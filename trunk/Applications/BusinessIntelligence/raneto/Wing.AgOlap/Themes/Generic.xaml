<ResourceDictionary  xmlns="http://schemas.microsoft.com/client/2007"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:vsm="clr-namespace:System.Windows;assembly=System.Windows" 
                     xmlns:controls="clr-namespace:Ranet.AgOlap.Controls.General"
                     xmlns:list="clr-namespace:Ranet.AgOlap.Controls.List"
                     xmlns:tree="clr-namespace:Ranet.AgOlap.Controls.General.Tree"
                     xmlns:buttons="clr-namespace:Ranet.AgOlap.Controls.Buttons"
                     xmlns:toolbar="clr-namespace:Ranet.AgOlap.Controls.ToolBar"
                     xmlns:combo="clr-namespace:Ranet.AgOlap.Controls.Combo"
                     xmlns:System="clr-namespace:System;assembly=mscorlib"
                     xmlns:basics="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
                     xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
                     xmlns:System_Windows_Controls_Primitives_Data="clr-namespace:System.Windows.Controls.Primitives;assembly=System.Windows.Controls.Data"
                     xmlns:data="clr-namespace:Ranet.AgOlap.Controls.Data"
