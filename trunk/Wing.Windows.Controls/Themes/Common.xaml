                                    IsTabStop="False" />
                            <RepeatButton x:Name="VerticalSmallIncrease" Grid.Row="4" Height="16"
                                    IsTabStop="False" Interval="50"
                                    Template="{StaticResource VerticalIncrementTemplate}" Margin="1" />
