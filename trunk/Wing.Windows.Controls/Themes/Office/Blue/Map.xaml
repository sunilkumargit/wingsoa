                                                <Image.Source>
                                                    <BitmapImage UriSource="/Telerik.Windows.Controls.DataVisualization;component/themes/map/road.png" />
                                                </Image.Source>
                                            </Image>
                                        </Grid>
                                    </telerikControls:RadRadioButton>
                                    <telerikControls:RadRadioButton x:Name="PART_MapStyle_Aerial" HorizontalContentAlignment="Stretch"
                                                                    telerikControls:StyleManager.Theme="{StaticResource Theme}"
                                                                    Tag="Aerial"
                                                                    Height="26"
                                                                    Margin="0,2"
                                                                    Opacity="{StaticResource MapDropDownButtonOpacity}"
                                                                    CornerRadius="3"
                                                                    Background="{StaticResource MapButtonBackground}"
					                                                BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}">

                                        <Grid HorizontalAlignment="Stretch">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition />
                                                <ColumnDefinition Width="32" />
                                            </Grid.ColumnDefinitions>

                                            <TextBlock telerikControls:LocalizationManager.ResourceKey="MapAerialCommand"
                                                       TextAlignment="Center" HorizontalAlignment="Stretch" Padding="7,0"
                                                       FontSize="11" FontWeight="Bold" Foreground="{TemplateBinding Foreground}" />
                                            <Image Grid.Column="1" Width="16" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                <Image.Source>
                                                    <BitmapImage UriSource="/Telerik.Windows.Controls.DataVisualization;component/themes/map/aerial.png" />
                                                </Image.Source>
                                            </Image>
                                        </Grid>
                                    </telerikControls:RadRadioButton>
                                    <telerikControls:RadRadioButton x:Name="PART_MapStyle_Birdseye" HorizontalContentAlignment="Stretch"
                                                                    telerikControls:StyleManager.Theme="{StaticResource Theme}"
                                                                    Tag="Birdseye"
                                                                    Height="26"
                                                                    Margin="0,2"
                                                                    Opacity="{StaticResource MapDropDownButtonOpacity}"
                                                                    CornerRadius="3"
                                                                    Background="{StaticResource MapButtonBackground}"
					                                                BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}">
                                        
                                        <Grid HorizontalAlignment="Stretch">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition />
                                                <ColumnDefinition Width="32" />
                                            </Grid.ColumnDefinitions>

                                            <TextBlock telerikControls:LocalizationManager.ResourceKey="MapBirdseyeCommand"
                                                       TextAlignment="Center" HorizontalAlignment="Stretch" Padding="7,0"
                                                       FontSize="11" FontWeight="Bold" Foreground="{TemplateBinding Foreground}" />
                                            <Image Grid.Column="1" Width="16" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                <Image.Source>
                                                    <BitmapImage UriSource="/Telerik.Windows.Controls.DataVisualization;component/themes/map/birdeye.png" />
                                                </Image.Source>
                                            </Image>
                                        </Grid>
                                    </telerikControls:RadRadioButton>
                                    <telerikControls:RadToggleButton x:Name="PART_IsLabelVisible" HorizontalContentAlignment="Stretch"
                                                                        telerikControls:StyleManager.Theme="{StaticResource Theme}" 
                                                                        Tag="Label" 
                                                                        Height="26"
                                                                        Margin="0,3"
                                                                        CornerRadius="3"
                                                                        Opacity="{StaticResource MapDropDownButtonOpacity}"
                                                                        Background="{StaticResource MapButtonBackground}"
					                                                    BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}">
                                        
                                        <Grid HorizontalAlignment="Stretch">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition />
                                                <ColumnDefinition Width="32" />
                                            </Grid.ColumnDefinitions>

                                            <TextBlock telerikControls:LocalizationManager.ResourceKey="MapLabelsVisibleCommand"
                                                       TextAlignment="Center" HorizontalAlignment="Stretch" Padding="7,0"
                                                       FontSize="11" FontWeight="Bold" Foreground="{TemplateBinding Foreground}" />
                                            <Image Grid.Column="1" Width="16" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                <Image.Source>
                                                    <BitmapImage UriSource="/Telerik.Windows.Controls.DataVisualization;component/themes/map/label.png" />
                                                </Image.Source>
                                            </Image>
                                        </Grid>
                                    </telerikControls:RadToggleButton>
                                </StackPanel>
                            </Grid>
                        </telerikControls:RadDropDownButton.DropDownContent>

                        <!-- Eye element -->
                        <Grid>
                            <Path Stretch="Fill" 
                                     StrokeThickness="2" 
                                     Margin="0,2,0,0" 
                                     Width="24" 
                                     Height="12" 
                                     Data="M23,6 C23,7.545 18.075132,11 12,11 5.9248678,11 1,6.955 1,6 1,5.045 5.9248678,1 12,1 18.075132,1 23,4.455 23,6 z" 
                                     Stroke="#FDFFFFFF"
                                     Fill="White"> 
                            </Path>
                            <Path Stretch="Fill" 
                                     StrokeThickness="2" 
                                     HorizontalAlignment="Center" 
                                     VerticalAlignment="Center" 
                                     Width="24" 
                                     Height="12" 
                                     Data="M23,6 C23,7.545 18.075132,11 12,11 5.9248678,11 1,6.955 1,6 1,5.045 5.9248678,1 12,1 18.075132,1 23,4.455 23,6 z"
                                     Stroke="{StaticResource MapEyePathStroke}"
                                     Fill="{StaticResource MapEyePathFill}" />
                            <Ellipse StrokeThickness="2" 
                                        HorizontalAlignment="Center" 
                                        VerticalAlignment="Center" 
                                        Width="7.162" 
                                        Height="7.162"
                                        Stroke="{StaticResource MapEyeEllipseStroke}"
                                        Fill="{StaticResource MapEyeEllipseFill}" />
                        </Grid>
                    </telerikControls:RadDropDownButton>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="maps:MapPinPoint"
		 >
		<Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="maps:MapPinPoint">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="1" 
                            CornerRadius="3"
                            Padding="2,2,2,2">
                        <StackPanel Name="PART_Panel" Orientation="Horizontal" Width="Auto">
                            <Image Name="PART_Image" Visibility="Collapsed" Source="{TemplateBinding ImageSource}" />
                            <TextBlock Name="PART_Text" Margin="2,0,0,0" VerticalAlignment="Center" Text="{TemplateBinding Text}" />
                        </StackPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

	<Style TargetType="maps:Pushpin"
		 >
        <Setter Property="Background" Value="{StaticResource MapPushpinBackground}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="maps:Pushpin">
                    <Border>
                        <maps:MapLayer.HotSpot>
							<maps:HotSpot X="0.5" Y="40" XUnits="Fraction" YUnits="InsetPixels" />
                        </maps:MapLayer.HotSpot>
                        <Path Stretch="Fill" 
                              Stroke="{StaticResource MapPushpinStroke}" 
                              StrokeThickness="6" 
                              Data="M12,26.083 L16,26.083 13.916667,32.083 z M14,3 C20.075132,3 25,7.9248676 25,14 25,20.075132 20.075132,25 14,25 7.9248677,25 3,20.075132 3,14 3,7.9248676 7.9248677,3 14,3 z"
                              Fill="{TemplateBinding Background}">
                            <Path.Effect>
                                
                                
                                    <DropShadowEffect BlurRadius="7" ShadowDepth="1" />
                                
                            </Path.Effect>
                        </Path>
                    </Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
		
	<DataTemplate x:Key="DefaultInformationLayerTemplate">
		<maps:Pushpin />
	</DataTemplate>

    <Style TargetType="maps:Balloon"
		 >
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="maps:Balloon">
                    <Border Background="{TemplateBinding Background}" 
                            BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="5">
                        <Grid Margin="5,5,5,5">
                            <ContentPresenter Content="{TemplateBinding Content}" 
                                              ContentTemplate="{TemplateBinding ContentTemplate}" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

	<Style TargetType="maps:MapMouseLocationIndicator"
		 >
		<Setter Property="FontFamily" Value="Arial" />
		<Setter Property="FontSize" Value="10" />
		<Setter Property="Foreground" Value="{StaticResource MapScaleForeground}" />
		<Setter Property="Background" Value="{StaticResource MapScaleBackground}" />
		<Setter Property="BorderBrush" Value="{StaticResource MapScaleBorderBrush}" />
		<Setter Property="StrokeThickness" Value="1" />

		<Setter Property="IndicatorLabelBackground" Value="{StaticResource MapScaleLabelBackground}" />
		<Setter Property="IndicatorLabelBorderBrush" Value="{StaticResource MapScaleLabelBorderBrush}" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="maps:MapMouseLocationIndicator">
					<Grid>
						<Border Grid.Column="1"
                                BorderBrush="{StaticResource MapScaleLabelOuterBorderBrush}"
                                BorderThickness="{StaticResource MapScaleLabelOuterBorderThickness}"
                                CornerRadius="{StaticResource MapScaleLabelOuterCornerRadius}"
                                Margin="5,0">
							<Border Background="{TemplateBinding IndicatorLabelBackground}"
                                    BorderBrush="{TemplateBinding IndicatorLabelBorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Padding="5,3"
                                    CornerRadius="{StaticResource MapScaleLabelCornerRadius}">
								<TextBlock x:Name="PART_TextBlock"
								           HorizontalAlignment="Center"
                                           VerticalAlignment="Center" />
							</Border>
						</Border>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	

</ResourceDictionary>
