    <SolidColorBrush x:Key="VerticalSingleLargeFill" Color="#00FFFFFF" />

    <!--Increase/Decrease Handle Style-->
    <Style x:Key="HandleStyle" TargetType="RepeatButton">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Grid x:Name="Root" Width="26" Height="36"> 
                        <Border x:Name="Border" BorderThickness="1"
								CornerRadius="{StaticResource SliderHandOuterBorder_CornerRadius}"
								BorderBrush="{StaticResource SliderHandOuterBorder_Normal}"
								Background="{StaticResource  SliderHandBackground_Normal}">
                            <Border x:Name="InnerBorder" BorderThickness="1"
									CornerRadius="{StaticResource SliderHandInnerBorder_CornerRadius}"
									BorderBrush="{StaticResource SliderHandInnerBorder_Normal}" />
                        </Border>

                        <telerikChromes:ButtonChrome
								telerikControls:StyleManager.Theme="{StaticResource Theme}"
								RenderNormal="False" RenderEnabled="{TemplateBinding IsEnabled}" Orientation="Vertical"
								RenderMouseOver="{TemplateBinding IsMouseOver}"
								RenderPressed="{TemplateBinding IsPressed}"
								RenderFocused="{TemplateBinding IsFocused}"
								CornerRadius="{StaticResource SliderHandOuterBorder_CornerRadius}" /> 
                        <ContentPresenter />
                    </Grid> 
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style> 

    <!--Vertical Thumb Template-->
    <ControlTemplate x:Key="VerticalThumbTemplate" TargetType="Thumb">
        <Grid>
            
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="Normal" />
                    <vsm:VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border"
									Storyboard.TargetProperty="BorderBrush" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlOuterBorderVertical_MouseOver}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBorder"
									Storyboard.TargetProperty="BorderBrush" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlInnerBorderVertical_MouseOver}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border"
									Storyboard.TargetProperty="Background" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlBackgroundVertical_MouseOver}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border"
									Storyboard.TargetProperty="BorderBrush" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlOuterBorderVertical_Pressed}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBorder"
									Storyboard.TargetProperty="BorderBrush" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlInnerBorderVertical_Pressed}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border"
									Storyboard.TargetProperty="Background" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlBackgroundVertical_Pressed}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Disabled">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border"
									Storyboard.TargetProperty="BorderBrush" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlOuterBorderVertical_Disabled}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBorder"
									Storyboard.TargetProperty="BorderBrush" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlInnerBorderVertical_Disabled}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border"
									Storyboard.TargetProperty="Background" Duration="0:0:0">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource ControlBackgroundVertical_Disabled}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>
            

            <Border x:Name="Border" BorderThickness="1"
					CornerRadius="{StaticResource ControlOuterBorder_CornerRadius}"
					BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}"
					Background="{StaticResource  ControlBackgroundVertical_Normal}">
                <Border x:Name="InnerBorder" BorderThickness="1"
						CornerRadius="{StaticResource ControlInnerBorder_CornerRadius}"
						BorderBrush="{StaticResource ControlInnerBorderVertical_Normal}" />
            </Border>
        </Grid>
        
    </ControlTemplate>

    <!--TickBar-->
    <ControlTemplate x:Key="TickBarTemplate" TargetType="telerikControls:RadTickBar">
        <Canvas x:Name="LayoutRoot" Background="Transparent" />
    </ControlTemplate>

    <!--RadSlider-->
    <Style x:Key="MapSlider" TargetType="telerikControls:RadSlider">
        <Setter Property="Orientation" Value="Vertical" />
        <Setter Property="IsEnabled" Value="True" />
        <Setter Property="HandlesVisibility" Value="Visible" />
        <Setter Property="IsMouseWheelEnabled" Value="True" />
        <Setter Property="Background" Value="{StaticResource SliderBackgroundTrack}" />
        <Setter Property="BorderBrush" Value="{StaticResource SliderOuterBorderTrack}" />
        <Setter Property="Delay" Value="250" />
        <Setter Property="RepeatInterval" Value="250" />
        <Setter Property="StepAction" Value="ChangeRange" />
        <Setter Property="ThumbVisibility" Value="Visible" />
        <Setter Property="SelectionStart" Value="0.4" />
        <Setter Property="SelectionEnd" Value="0.6" />
        <Setter Property="MinimumRangeSpan" Value="0.0" />
        <Setter Property="IsSnapToTickEnabled" Value="True" />
        <Setter Property="TickPlacement" Value="None" />
        <Setter Property="TickFrequency" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControls:RadSlider">
                    <Grid x:Name="LayoutRoot">
                        <Grid.Resources>
                            <!-- Tick Template --> 
                            <DataTemplate x:Key="VerticalTickTemplate">
                                <Grid x:Name="RootElement" ToolTipService.ToolTip="{Binding}">
                                    <Rectangle Width="6" Height="1"
											Fill="{StaticResource SliderBackground_Tick}" />
                                    <Rectangle Width="6" Height="1" Margin="0,1,0,0"
											Fill="{StaticResource SliderBackground_TickShadow}" />
                                </Grid>
                            </DataTemplate>
                        </Grid.Resources>
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="FocusVisual" To="1"
												Duration="0" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unfocused">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="FocusVisual" To="0"
												Duration="0" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <!-- input valid states -->
                            <vsm:VisualStateGroup x:Name="ValidationStates">
                                <vsm:VisualState x:Name="Valid" />
                                <vsm:VisualState x:Name="InvalidUnfocused">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ValidationErrorElement"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="InvalidFocused">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ValidationErrorElement"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="validationTooltip"
												Storyboard.TargetProperty="IsOpen">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.1">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <system:Boolean>True</system:Boolean>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Grid> 
                            <!--VerticalTemplate-->
                            <Grid x:Name="VerticalTemplate">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>

                                <!--[Vertical] [LeftTicks]-->
                                <telerikControls:RadTickBar x:Name="LeftTickBar" IsTabStop="False"
										Template="{StaticResource TickBarTemplate}"  MinWidth="6"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										Placement="Left" Grid.Column="0" Grid.Row="1"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />
                                <!--[Vertical] [RightTicks]-->
                                <telerikControls:RadTickBar x:Name="RightTickBar" IsTabStop="False"
										Template="{StaticResource TickBarTemplate}" MinWidth="6"
										Placement="Right" Grid.Column="2" Grid.Row="1"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />

                                <!--Track-->
                                <Border Width="6" HorizontalAlignment="Center"
										CornerRadius="{StaticResource SliderOuterBorderTrack_CornerRadius}"
										BorderThickness="{StaticResource SliderOuterBorderTrack_BorderThicknessVertical}"
										BorderBrush="{TemplateBinding BorderBrush}" 
                                        Grid.Column="1"
										Grid.Row="1">
                                    <Border CornerRadius="{StaticResource SliderInnerBorderTrack_CornerRadius}"
											BorderThickness="{StaticResource SliderInnerBorderTrack_BorderThickness}"
											BorderBrush="{StaticResource SliderInnerBorderTrack}"
											Background="{TemplateBinding Background}" />
                                </Border> 
                                <!--[Vertical] [TrackBackground]-->

                                <!--[Vertical] [SingleThumbTemplate]-->
                                <Grid x:Name="VerticalSingleThumbTemplate" 
                                         Grid.Row="1"
										 Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Rectangle x:Name="VerticalSingleLargeDecrease" Cursor="Hand"
											Grid.Row="2" Opacity="0"
											Fill="{StaticResource VerticalSingleLargeFill}"
											HorizontalAlignment="Stretch" />
                                    <Thumb x:Name="VerticalSingleThumb"
											IsEnabled="{TemplateBinding IsEnabled}" 
                                            Width="20"
											Height="10" 
                                            Grid.Row="1"
											Template="{StaticResource VerticalThumbTemplate}"
											HorizontalAlignment="Center"
											Visibility="{TemplateBinding ThumbVisibility}" />
                                    <Rectangle x:Name="VerticalSingleLargeIncrease" Cursor="Hand"
											Grid.Row="0" Opacity="0"
											Fill="{StaticResource VerticalSingleLargeFill}"
											HorizontalAlignment="Stretch" />
                                </Grid>

                                <!--[Vertical] [DecreaseHandle]-->
                                <RepeatButton x:Name="VerticalDecreaseHandle"
										IsEnabled="{TemplateBinding IsEnabled}"
										Delay="{TemplateBinding Delay}"
										Interval="{TemplateBinding RepeatInterval}"
										HorizontalAlignment="Center" Grid.Column="1" Grid.Row="2"
										Visibility="{TemplateBinding HandlesVisibility}"
										Style="{StaticResource HandleStyle}" 
										RenderTransformOrigin="0.5,0.5"
                                        Opacity="0.8">
                                    <RepeatButton.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform ScaleX="-1" ScaleY="-1" />
                                            <RotateTransform Angle="90" />
                                        </TransformGroup>
                                    </RepeatButton.RenderTransform>
                                    <Path Fill="{StaticResource ControlElement_Normal}" 
								            Stretch="Fill" HorizontalAlignment="Center"
								            VerticalAlignment="Center" Width="2" Height="9" 
								            Data="M1.48956,16.175882 L7.1873875,16.175882 L7.1873875,18.47793 L1.48956,18.47793 z" />
