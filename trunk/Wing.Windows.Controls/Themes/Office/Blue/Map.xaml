                                    <Path Fill="{StaticResource ControlElement_Normal}" 
								            Stretch="Fill" HorizontalAlignment="Center"
								            VerticalAlignment="Center" Width="9" Height="9" 
								            Data="M4.0009999,0 L6.0009999,0 6.0009999,3.9619999 10,3.9619999 10,5.9619999 6.0009999,5.9619999 6.0009999,10 4.0009999,10 4.0009999,5.9619999 0,5.9619999 0,3.9619999 4.0009999,3.9619999 z" />
                                </RepeatButton>
                            </Grid>

