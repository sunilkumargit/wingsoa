                        <telerikControls:RadDropDownButton.DropDownContent>
                            
                            <Grid x:Name="PART_DropDownPopupWrapper" Background="#01ffffff" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                <Border HorizontalAlignment="Right" 
                                        Width="42" 
                                        BorderBrush="{StaticResource MapDropDownOuterBorderBrush}"
                                        Background="{StaticResource MapDropDownOuterBackground}"
                                        BorderThickness="1,1,1,0" 
                                        CornerRadius="3">
                                    <Border Width="40"
                                            Background="{StaticResource MapDropDownBackground}"
                                            BorderBrush="{StaticResource MapDropDownBorderBrush}" 
                                            BorderThickness="1,1,1,0" 
                                            CornerRadius="2" />
                                </Border>

                                <StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Margin="0,5,5,5">
                                    <telerikControls:RadRadioButton x:Name="PART_MapStyle_Road" HorizontalContentAlignment="Stretch"
