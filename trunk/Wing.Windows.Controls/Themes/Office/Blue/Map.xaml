                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition />
                                                <ColumnDefinition Width="32" />
                                            </Grid.ColumnDefinitions>

                                            <TextBlock telerikControls:LocalizationManager.ResourceKey="MapRoadCommand"
