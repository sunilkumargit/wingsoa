                                                               Margin="0,1"
                                                               Opacity="{StaticResource MapDropDownButtonOpacity}"
                                                               CornerRadius="3"
                                                               Padding="7,0"
                                                               FontSize="11"
                                                               FontWeight="Bold"
                                                               Background="{StaticResource MapButtonBackground}"
				                                               BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}"/>
                                        <telerikControls:RadButton telerikControls:StyleManager.Theme="{StaticResource Theme}"
                                                               telerikControls:LocalizationManager.ResourceKey="MapPresetZoomLevelCity"
                                                               Tag="CityLevel"
                                                               Height="24"
                                                               Margin="0,1"
                                                               Opacity="{StaticResource MapDropDownButtonOpacity}"
                                                               CornerRadius="3"
                                                               Padding="7,0"
                                                               FontSize="11"
                                                               FontWeight="Bold"
                                                               Background="{StaticResource MapButtonBackground}"
				                                               BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}" />
                                        <telerikControls:RadButton telerikControls:StyleManager.Theme="{StaticResource Theme}"
                                                               telerikControls:LocalizationManager.ResourceKey="MapPresetZoomLevelRegion"
                                                               Tag="RegionLevel"
                                                               Height="24"
                                                               Margin="0,1"
                                                               Opacity="{StaticResource MapDropDownButtonOpacity}"
                                                               CornerRadius="3"
                                                               Padding="7,0"
                                                               FontSize="11"
                                                               FontWeight="Bold"
                                                               Background="{StaticResource MapButtonBackground}"
				                                               BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}" />
                                        <telerikControls:RadButton telerikControls:StyleManager.Theme="{StaticResource Theme}"
                                                               telerikControls:LocalizationManager.ResourceKey="MapPresetZoomLevelState"
                                                               Tag="StateLevel"
                                                               Height="24"
                                                               Margin="0,1"
                                                               Opacity="{StaticResource MapDropDownButtonOpacity}"
                                                               CornerRadius="3"
                                                               Padding="10,0"
                                                               FontSize="11"
                                                               FontWeight="Bold"
                                                               Background="{StaticResource MapButtonBackground}"
				                                               BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}" />
                                        <telerikControls:RadButton telerikControls:StyleManager.Theme="{StaticResource Theme}"
                                                               telerikControls:LocalizationManager.ResourceKey="MapPresetZoomLevelWorld"
                                                               Tag="WorldLevel"
                                                               Height="24"
                                                               Margin="0,1"
                                                               Opacity="{StaticResource MapDropDownButtonOpacity}"
                                                               CornerRadius="3"
                                                               Padding="7,0"
                                                               FontSize="11"
                                                               FontWeight="Bold"
                                                               Background="{StaticResource MapButtonBackground}"
				                                               BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}"  />
                                    </StackPanel>
                                </Grid>
                            </telerikControls:RadDropDownButton.DropDownContent>

                            <!-- Magnifier element -->
                            <Path Stretch="Fill" StrokeThickness="2" Width="22" Height="22" 
                                     Data="M13.952573,12.273359 L21.023642,19.588496 19.609426,21.00271 12.53836,13.687573 z M8,1 C11.865993,1.0000001 15,4.1340071 15,8.0000001 15,11.865994 11.865993,15 8,15 4.134007,15 1,11.865994 1,8.0000001 1,4.1340071 4.134007,1.0000001 8,1 z"
                                     Stroke="{StaticResource MapMagnifierPathStroke}"
                                     Fill="{StaticResource MapMagnifierPathFill}">
                                <Path.Effect>
                                    
                                    
                                    <DropShadowEffect BlurRadius="3" Color="White" ShadowDepth="0" />
                                    
                                </Path.Effect>
                            </Path>
                        </telerikControls:RadDropDownButton>

                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
