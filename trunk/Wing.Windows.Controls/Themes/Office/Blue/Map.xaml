    <Style TargetType="maps:MapZoomBar"
		 >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="maps:MapZoomBar">
                    <Grid x:Name="mapNavigation" Background="Transparent">
                        <telerikControls:RadDropDownButton x:Name="PART_MapNavigationDropDown"
                                                           telerikControls:StyleManager.Theme="{StaticResource Theme}"
                                                           Grid.Column="1" 
                                                           HorizontalAlignment="Center" 
                                                           VerticalAlignment="Center" 
                                                           ClickMode="Hover" 
                                                           CloseOnEscape="True"  
                                                           DropDownPlacement="Top" 
                                                           DropDownIndicatorVisibility="Collapsed" 
                                                           DropDownHeight="200"
                                                           Width="36"
                                                           Height="26" 
                                                           CornerRadius="3"
                                                           Background="{StaticResource MapDropDownButtonBackground}"
					                                       BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}"> 
                            <telerikControls:RadDropDownButton.DropDownContent>
                                <Grid x:Name="PART_DropDownPopupWrapper" Background="#01ffffff" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                    <Border BorderBrush="{StaticResource MapDropDownOuterBorderBrush}"
                                                Background="{StaticResource MapDropDownOuterBackground}"
                                                BorderThickness="1,1,1,0" 
                                                CornerRadius="3" 
                                                HorizontalAlignment="Left"
                                                Width="42">
                                        <Border Background="{StaticResource MapDropDownBackground}"
                                                BorderBrush="{StaticResource MapDropDownBorderBrush}" 
                                                BorderThickness="1,1,1,0" 
                                                CornerRadius="2"
                                                HorizontalAlignment="Center"
                                                Width="40" />
                                    </Border>
                                    
                                    <telerikControls:RadSlider Grid.ColumnSpan="2" x:Name="PART_ZoomSlider" Margin="8,0,0,0"
                                                           Style="{StaticResource MapSlider}"
                                                           HorizontalAlignment="Left" VerticalAlignment="Stretch" 
									                       Minimum="1" 
									                       Maximum="20" />

                                    <StackPanel x:Name="PART_ZoomPresetsPanel" Margin="35,0,0,0"
                                                Grid.Column="1" Grid.ColumnSpan="2" VerticalAlignment="Center" HorizontalAlignment="Left">
                                        <telerikControls:RadButton telerikControls:StyleManager.Theme="{StaticResource Theme}"
                                                               telerikControls:LocalizationManager.ResourceKey="MapPresetZoomLevelNeighborhood"
                                                               Tag="NeighborhoodLevel"
