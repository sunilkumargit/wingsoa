                                </RepeatButton>
                                <!--[Vertical] [IncreaseHandle]-->
                                <RepeatButton x:Name="VerticalIncreaseHandle" IsTabStop="False"
										IsEnabled="{TemplateBinding IsEnabled}"
										Delay="{TemplateBinding Delay}"
										Interval="{TemplateBinding RepeatInterval}"
										HorizontalAlignment="Center"
										Grid.Column="1" 
                                        Grid.Row="0"
                                        Width="{StaticResource RadSliderHandleHeight}" 
                                        Height="{StaticResource RadSliderHandleWidth}" 
										Visibility="{TemplateBinding HandlesVisibility}"
										Style="{StaticResource HandleStyle}">
                                    <Path Fill="{StaticResource RadSliderVerticalHandlePathFill}"
											Stretch="Fill"
											Stroke="{StaticResource RadSliderHandlePathStroke}"
											StrokeThickness="0" 
                                            HorizontalAlignment="Center"
											VerticalAlignment="Center" 
                                            Width="7" 
                                            Height="4"
											Data="M2.0000005,1.0000001 L5.0000005,1.0000001 L5.0000005,2 L6.0000005,2 L6.0000005,2.9999998 L7,2.9999998 L7,3.9999998 L0,3.9999998 L0,2.9999998 L1.0000006,2.9999998 L1.0000006,2 L2.0000005,2 z M3.0000007,0 L4.000001,0 L4.000001,1 L3.0000007,1 z" />
                                </RepeatButton>
                            </Grid>

                        </Grid>
                        <Rectangle x:Name="FocusVisual" Stroke="{StaticResource FocusBrushBlack}"
								Opacity="0" StrokeDashArray="1 2" StrokeThickness="1" RadiusX="2"
								RadiusY="2" />
                        <!--Error Visual Element-->

                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
