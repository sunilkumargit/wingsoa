<SolidColorBrush x:Key="StripLinesDiffuseMaterialBrush2" Color="#FFEBF7FF"/>
<SolidColorBrush x:Key="StripLinesDiffuseMaterialBrush3" Color="#FFDCECF7"/>

    

    <SolidColorBrush x:Key="VerticalSingleLargeFill" Color="#00FFFFFF" />

    

    <!--Increase/Decrease Handle Style-->
    <Style x:Key="HandleStyle" TargetType="RepeatButton">
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Grid x:Name="Root">
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterPath"
												Storyboard.TargetProperty="BorderBrush" Duration="0:0:1">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadSliderHandleOuterStroke_Over}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Path"
												Storyboard.TargetProperty="Fill" Duration="0:0:1">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadSliderHandleFill_Over}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Path"
												Storyboard.TargetProperty="Stroke" Duration="0:0:1">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadSliderHandleStroke_Over}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterPath"
												Storyboard.TargetProperty="BorderBrush" Duration="0:0:1">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadSliderHandleOuterStroke_Pressed}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Path"
												Storyboard.TargetProperty="Fill" Duration="0:0:2">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadSliderHandleFill_Pressed}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Path"
												Storyboard.TargetProperty="Stroke" Duration="0:0:1">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadSliderHandleStroke_Pressed}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Disabled" />
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="FocusStates">
                                <vsm:VisualState x:Name="Focused" />
                                <vsm:VisualState x:Name="Unfocused" />
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Border x:Name="OuterPath" 
                                     CornerRadius="{StaticResource RadSliderHandleOuterRadius}"
                                     BorderThickness="1" 
                                     BorderBrush="{StaticResource RadSliderHandleOuterStroke}">
                            <Rectangle x:Name="Path"
                                     RadiusX="{StaticResource RadSliderHandleRadius}"
                                     RadiusY="{StaticResource RadSliderHandleRadius}"
                                     StrokeThickness="1" 
                                     Stroke="{StaticResource RadSliderHandleStroke}"
								     Fill="{StaticResource RadSliderHandleFill}" />
                        </Border>

                        <ContentPresenter />

                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--Horizontal Thumb Template-->
    <ControlTemplate x:Key="HorizontalThumbTemplate" TargetType="Thumb">
        <Grid Margin="3,0">
            <Path x:Name="Path" 
                   Width="{StaticResource RadSliderHorizontalThumbWidth}"
                   Height="{StaticResource RadSliderHorizontalThumbHeight}" 
                   Fill="{StaticResource RadSliderThumbFill}"
				   Data="M4,0 L5,0 5,9 4,9 z M2,0 L3,0 3,9 2,9 z M0,0 L1.0000001,0 1.0000001,9 0,9 z"
                   />
                <Path Margin="2,0,0,0"
                       Width="{StaticResource RadSliderHorizontalThumbWidth}"
                       Height="{StaticResource RadSliderHorizontalThumbHeight}" 
                       Fill="{StaticResource RadSliderThumbShadowFill}"
			Data="M4,0 L5,0 5,9 4,9 z M2,0 L3,0 3,9 2,9 z M0,0 L1.0000001,0 1.0000001,9 0,9 z"
                   />
        </Grid>
    </ControlTemplate>

    <!--Horizontal Range Selection Thumb Template-->
    <ControlTemplate x:Key="HorizontalRangeSelectionThumbTemplate" TargetType="Thumb">
        <Grid>
            
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="Normal" />
                    <vsm:VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetProperty="Opacity"
									Storyboard.TargetName="border" To="1" Duration="0:0:0.2" />
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Pressed" />
                    <vsm:VisualState x:Name="Disabled" />
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>
            
            <Border x:Name="border" 
                     Height="{StaticResource RadSliderRangeThumbSize}" 
		             Opacity="1"
                     VerticalAlignment="Center"
                     CornerRadius="{StaticResource RadSliderRangeThumbCornerRadius}"
                     BorderThickness="{StaticResource RadSliderRangeThumbBorderThickness}"
                     BorderBrush="{StaticResource RadSliderHorizontalRangeThumbBorderBrush}"
					 Background="{StaticResource RadSliderHorizontalRangeThumbBackground}" />

        </Grid>
        
    </ControlTemplate>

    <!--Vertical Thumb Template-->
    <ControlTemplate x:Key="VerticalThumbTemplate" TargetType="Thumb">
        <Grid Margin="0,3">
            <Path x:Name="Path" 
                        Width="{StaticResource RadSliderVerticalThumbWidth}"
                        Height="{StaticResource RadSliderVerticalThumbHeight}" 
                        Fill="{StaticResource RadSliderThumbFill}"
				        Data="M0,4 L8,4 8,5 0,5 z M0,2 L8,2 8,3 0,3 z M0,0 L8,0 8,1 0,1 z"
                       />
                <Path  Margin="0,2,0,0"
                        Width="{StaticResource RadSliderVerticalThumbWidth}"
                        Height="{StaticResource RadSliderVerticalThumbHeight}" 
                        Fill="{StaticResource RadSliderThumbShadowFill}"
				        Data="M0,4 L8,4 8,5 0,5 z M0,2 L8,2 8,3 0,3 z M0,0 L8,0 8,1 0,1 z"
                        />
        </Grid>
    </ControlTemplate>

    <!--Vertical Range Selection Thumb Template-->
    <ControlTemplate x:Key="VerticalRangeSelectionThumbTemplate" TargetType="Thumb">
        <Grid>
            
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="Normal" />
                    <vsm:VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetProperty="Opacity"
									Storyboard.TargetName="border" To="1" Duration="0:0:0.2" />
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Pressed" />
                    <vsm:VisualState x:Name="Disabled" />
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>
            
            <Border x:Name="border" 
                     Width="{StaticResource RadSliderRangeThumbSize}" 
                     HorizontalAlignment="Center"
                     CornerRadius="{StaticResource RadSliderRangeThumbCornerRadius}"
                     BorderThickness="{StaticResource RadSliderRangeThumbBorderThickness}"
                     BorderBrush="{StaticResource RadSliderVerticalRangeThumbBorderBrush}"
					 Background="{StaticResource RadSliderVerticalRangeThumbBackground}" />
        </Grid>
        
    </ControlTemplate>

    <!--TickBar-->
    <ControlTemplate x:Key="TickBarTemplate" TargetType="telerikControls:RadTickBar">
        <Canvas x:Name="LayoutRoot" Background="Transparent" />
    </ControlTemplate>

    <!--RadSlider-->
    <Style TargetType="telerikControls:RadSlider"
		   x:Key="ChartSlider">
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="Orientation" Value="Horizontal" />
        <Setter Property="IsEnabled" Value="True" />
        <Setter Property="HandlesVisibility" Value="Visible" />
        <Setter Property="IsMouseWheelEnabled" Value="True" />
        <Setter Property="Background" Value="{StaticResource RadSliderTrackBackground}" />
        <Setter Property="BorderBrush" Value="{StaticResource RadSliderTrackBorder}" />
        <Setter Property="Delay" Value="250" />
        <Setter Property="RepeatInterval" Value="250" />
        <Setter Property="StepAction" Value="ChangeRange" />
        <Setter Property="ThumbVisibility" Value="Visible" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControls:RadSlider">
                    <Grid x:Name="LayoutRoot">
                        <Grid.Resources>
                            <!-- Tick Template -->
                            <DataTemplate x:Key="HorizontalTickTemplate">
                                <Grid x:Name="RootElement" ToolTipService.ToolTip="{Binding}">
                                    <Rectangle Width="1" Height="5"
											Fill="{StaticResource RadSliderBackground_tick}" />
                                </Grid>
                            </DataTemplate>
                            <DataTemplate x:Key="VerticalTickTemplate">
                                <Grid x:Name="RootElement" ToolTipService.ToolTip="{Binding}">
                                    <Rectangle Width="5" Height="1"
											Fill="{StaticResource RadSliderBackground_tick}" />
                                </Grid>
                            </DataTemplate>
                        </Grid.Resources>
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <!--FocusStates-->
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="FocusVisual" To="1" Duration="0"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unfocused">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="FocusVisual" To="0" Duration="0"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <!-- input valid states -->
                            <vsm:VisualStateGroup x:Name="ValidationStates">
                                <vsm:VisualState x:Name="Valid" />
                                <vsm:VisualState x:Name="InvalidUnfocused">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ValidationErrorElement"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="InvalidFocused">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ValidationErrorElement"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="validationTooltip"
												Storyboard.TargetProperty="IsOpen">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.1">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <mscorlib:Boolean>True</mscorlib:Boolean>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Grid>

                            <!--HorizontalTemplate-->
                            <Grid x:Name="HorizontalTemplate">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>

                                <Border Grid.RowSpan="3" 
                                           Grid.ColumnSpan="3" 
                                           CornerRadius="{StaticResource RadSliderCornerRadius}"
                                           BorderThickness="{StaticResource RadSliderHorizontalBorderThickness}" 
                                           BorderBrush="{StaticResource RadSliderBorderBrush}" >
                                    <Border CornerRadius="{StaticResource RadSliderInnerCornerRadius}" 
                                               BorderThickness="{StaticResource RadSliderInnerBorderThickness}" 
                                               BorderBrush="{StaticResource RadSliderHorizontalInnerBorderBrush}" 
                                               Background="{StaticResource RadSliderHorizontalBackground}" />
                                </Border>

                                <!--[Horizontal] [TopTicks]-->
                                <telerikControls:RadTickBar x:Name="TopTickBar" IsTabStop="False"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										Template="{StaticResource TickBarTemplate}" MinHeight="6"
										Placement="Top" Grid.Column="1"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />
                                <!--[Horizontal] [BottomTicks]-->
                                <telerikControls:RadTickBar x:Name="BottomTickBar" IsTabStop="False"
										Template="{StaticResource TickBarTemplate}" MinHeight="6"
										Placement="Bottom" Grid.Column="1" Grid.Row="2"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />

                                <!--Track-->
                                <Border Grid.Row="1"
                                                Grid.Column="1"
                                                VerticalAlignment="Center" 
                                                Height="9"
										        Background="{TemplateBinding Background}" 
										        BorderThickness="1"
										        BorderBrush="{TemplateBinding BorderBrush}" >
                                </Border>

                                <!--[Horizontal] [SingleThumbTemplate]-->
                                <Grid x:Name="HorizontalSingleThumbTemplate" Grid.Row="1"
										Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle x:Name="HorizontalSingleLargeDecrease" Cursor="Hand"
											Opacity="0" Fill="Transparent"
											VerticalAlignment="Stretch" />
                                    <Thumb x:Name="HorizontalSingleThumb"
											IsEnabled="{TemplateBinding IsEnabled}" 
                                            Grid.Column="1"
											Template="{StaticResource HorizontalThumbTemplate}"
                                            Cursor="SizeWE"
											Visibility="{TemplateBinding ThumbVisibility}" />
                                    <Rectangle x:Name="HorizontalSingleLargeIncrease" Cursor="Hand"
											Grid.Column="2" Opacity="0" Fill="Transparent"
											VerticalAlignment="Stretch" />
                                </Grid>

                                <!--[Horizontal] [RangeSelectionTemplate]-->
                                <Grid x:Name="HorizontalRangeTemplate" Grid.Row="1" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>

                                    <Rectangle x:Name="HorizontalRangeLargeDecrease" Cursor="Hand"
											Opacity="0" Fill="Transparent" Grid.Column="0" />
                                    <Border Grid.Column="1"
                                                Grid.ColumnSpan="3"
                                                BorderThickness="1"
                                                BorderBrush="{StaticResource RadSliderThumbsOuterBorderBrush}"
                                                CornerRadius="{StaticResource RadSliderThumbsOuterCornerRadius}">
                                        <Border BorderThickness="1"
                                                    BorderBrush="{StaticResource RadSliderThumbsBorderBrush}"
                                                    Background="{StaticResource RadSliderHorizontalThumbsBackground}"
                                                    CornerRadius="{StaticResource RadSliderThumbsCornerRadius}" />
                                    </Border>
                                    <Thumb x:Name="HorizontalRangeStartThumb"
											IsEnabled="{TemplateBinding IsEnabled}"
                                            Grid.Column="1"
											Template="{StaticResource HorizontalThumbTemplate}"
                                            Cursor="SizeWE"
											Visibility="{TemplateBinding ThumbVisibility}" />
                                    <Thumb x:Name="HorizontalRangeMiddleThumb"
											IsEnabled="{TemplateBinding IsEnabled}" 
                                            Grid.Column="2"
											Template="{StaticResource HorizontalRangeSelectionThumbTemplate}"
                                            Cursor="Hand" />
                                    <Thumb x:Name="HorizontalRangeEndThumb"
											IsEnabled="{TemplateBinding IsEnabled}"
                                            Grid.Column="3"
											Template="{StaticResource HorizontalThumbTemplate}"
                                            Cursor="SizeWE"
											Visibility="{TemplateBinding ThumbVisibility}" />
                                    <Rectangle x:Name="HorizontalRangeLargeIncrease" Cursor="Hand"
											Opacity="0" Fill="Transparent" Grid.Column="4" />
                                </Grid>

                                <!--[Horizontal] [DecreaseHandle]-->
                                <RepeatButton x:Name="HorizontalDecreaseHandle" IsTabStop="False"
										    IsEnabled="{TemplateBinding IsEnabled}"
										    Delay="{TemplateBinding Delay}"
										    Interval="{TemplateBinding RepeatInterval}"  
                                            Grid.Row="1"
                                            Width="{StaticResource RadSliderHandleWidth}" 
                                            Height="{StaticResource RadSliderHandleHeight}" 
										    Visibility="{TemplateBinding HandlesVisibility}"
										    Style="{StaticResource HandleStyle}" >
                                    <Path Stroke="{StaticResource RadSliderHandlePathStroke}"
											  HorizontalAlignment="Center" 
                                              VerticalAlignment="Center"
											  Width="4" 
                                              Height="7"
											  Fill="{StaticResource RadSliderHorizontalHandlePathFill}"
											  StrokeThickness="0"
                                              Data="M0,3.0000002 L1,3.0000002 L1,4 L0,4 z M2,1.0000002 L3,1.0000002 L3,6 L2,6 L2,5 L1.0000002,5 L1.0000002,2.0000002 L2,2.0000002 z M3.0000002,0 L4,0 L4,7 L3.0000002,7 z" />
                                </RepeatButton>
                                <!--[Horizontal] [IncreaseHandle]-->
                                <RepeatButton x:Name="HorizontalIncreaseHandle" IsTabStop="False"
										    IsEnabled="{TemplateBinding IsEnabled}"
										    Delay="{TemplateBinding Delay}"
										    Interval="{TemplateBinding RepeatInterval}"
                                            Grid.Row="1" 
                                            Grid.Column="2"
                                            Width="{StaticResource RadSliderHandleWidth}" 
                                            Height="{StaticResource RadSliderHandleHeight}" 
										    Visibility="{TemplateBinding HandlesVisibility}"
										    Style="{StaticResource HandleStyle}" >
                                    <Path Fill="{StaticResource RadSliderHorizontalHandlePathFill}"
											 Stretch="Fill"
											 Stroke="{StaticResource RadSliderHandlePathStroke}"
											 StrokeThickness="0" 
                                             HorizontalAlignment="Center"
											 VerticalAlignment="Center" 
                                             Width="4" 
                                             Height="7"
											 Data="M2.0000002,2.0000002 L3.0000002,2.0000002 L3.0000002,3.0000002 L4,3.0000002 L4,4 L3.0000002,4 L3.0000002,5 L2.0000002,5 z M1.0000001,1.0000002 L2,1.0000002 L2,6 L1.0000001,6 z M0,0 L1,0 L1,7 L0,7 z" />
                                </RepeatButton>
                            </Grid>

                            <!--VerticalTemplate-->
                            <Grid x:Name="VerticalTemplate">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>

                                <Border Grid.RowSpan="3" 
                                           Grid.ColumnSpan="3" 
                                           CornerRadius="{StaticResource RadSliderCornerRadius}"
                                           BorderThickness="{StaticResource RadSliderVerticalBorderThickness}" 
                                           BorderBrush="{StaticResource RadSliderBorderBrush}" >
                                    <Border CornerRadius="{StaticResource RadSliderInnerCornerRadius}" 
                                               BorderThickness="{StaticResource RadSliderInnerBorderThickness}" 
                                               BorderBrush="{StaticResource RadSliderVerticalInnerBorderBrush}" 
                                               Background="{StaticResource RadSliderVerticalBackground}" />
                                </Border>

                                <!--[Vertical] [LeftTicks]-->
                                <telerikControls:RadTickBar x:Name="LeftTickBar" IsTabStop="False"
										Template="{StaticResource TickBarTemplate}" MinWidth="6"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										Placement="Left" Grid.Column="0" Grid.Row="1"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />
                                <!--[Vertical] [RightTicks]-->
                                <telerikControls:RadTickBar x:Name="RightTickBar" IsTabStop="False"
										Template="{StaticResource TickBarTemplate}" MinWidth="6"
										Placement="Right" Grid.Column="2" Grid.Row="1"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />

                                <!--Track-->
                                <Border Grid.Column="1"
										   Grid.Row="1" 
										   HorizontalAlignment="Center"
                                           Width="9"
                                           Background="{TemplateBinding Background}"
										   BorderThickness="1"
										   BorderBrush="{TemplateBinding BorderBrush}">
                                </Border>

                                <!--[Vertical] [SingleThumbTemplate]-->
                                <Grid x:Name="VerticalSingleThumbTemplate" Grid.Row="1"
										Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Rectangle x:Name="VerticalSingleLargeDecrease" Cursor="Hand"
											Grid.Row="2" Opacity="0"
											Fill="{StaticResource VerticalSingleLargeFill}"
											HorizontalAlignment="Stretch" />
                                    <Thumb x:Name="VerticalSingleThumb"
											IsEnabled="{TemplateBinding IsEnabled}"
                                            Grid.Row="1"
											Template="{StaticResource VerticalThumbTemplate}"
                                            Cursor="SizeNS"
											Visibility="{TemplateBinding ThumbVisibility}" />
                                    <Rectangle x:Name="VerticalSingleLargeIncrease" Cursor="Hand"
											Grid.Row="0" Opacity="0"
											Fill="{StaticResource VerticalSingleLargeFill}"
											HorizontalAlignment="Stretch" />
                                </Grid>

                                <!--[Vertical] [RangeSelectionTemplate]-->
                                <Grid x:Name="VerticalRangeTemplate" Grid.Row="1" Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>

                                    <Rectangle x:Name="VerticalRangeLargeDecrease" Cursor="Hand"
											Opacity="0" Fill="Transparent" Grid.Row="4" />
                                    <Border Grid.Row="1"
                                                Grid.RowSpan="3"
                                                BorderThickness="1"
                                                BorderBrush="{StaticResource RadSliderThumbsOuterBorderBrush}"
                                                CornerRadius="{StaticResource RadSliderThumbsOuterCornerRadius}">
                                        <Border BorderThickness="1"
                                                    BorderBrush="{StaticResource RadSliderThumbsBorderBrush}"
                                                    Background="{StaticResource RadSliderVerticalThumbsBackground}"
                                                    CornerRadius="{StaticResource RadSliderThumbsCornerRadius}" />
                                    </Border>
                                    <Thumb x:Name="VerticalRangeStartThumb"
											IsEnabled="{TemplateBinding IsEnabled}"
                                            Grid.Row="3"
											Template="{StaticResource VerticalThumbTemplate}"
                                            Cursor="SizeNS"
											Visibility="{TemplateBinding ThumbVisibility}" />
                                    <Thumb x:Name="VerticalRangeMiddleThumb"
											IsEnabled="{TemplateBinding IsEnabled}" 
                                            Grid.Row="2"
											Template="{StaticResource VerticalRangeSelectionThumbTemplate}"
                                            Cursor="Hand" />
                                    <Thumb x:Name="VerticalRangeEndThumb"
											IsEnabled="{TemplateBinding IsEnabled}"
                                            Grid.Row="1"
