<SolidColorBrush x:Key="BarMaskStroke" Color="Transparent" />
<mscorlib:Double x:Key="BarMaskStrokeThickness">0</mscorlib:Double>
<SolidColorBrush x:Key="BarTopMaskBrush" Color="Transparent" />
<SolidColorBrush x:Key="BarOpacityMaskReversedBrush" Color="#FF000000" />
