    <Style TargetType="gauges:Needle"
        >
        <Setter Property="Location" Value="OverCenter" />
        <Setter Property="RelativeHeight" Value="0.2" />
        <Setter Property="RelativeShift" Value="0.8" />
        <Setter Property="Background" Value="{StaticResource NeedleEllipseBackground}" />
       <Setter Property="BorderBrush" Value="{StaticResource NeedleEllipseStroke}" /> 
       <Setter Property="ArrowBackground" Value="{StaticResource NeedleMaskBackground}" />
       <Setter Property="ArrowBorderBrush" Value="{StaticResource NeedleArrowBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gauges:Needle">
                    <Grid x:Name="PART_Grid" 
                          HorizontalAlignment="Stretch" 
                          VerticalAlignment="Stretch">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <!-- Column 0 - tail -->
                            <ColumnDefinition Width="*"/>
                            <!-- Column 1 - pin point -->
                            <ColumnDefinition Width="*"/>
                            <!-- Column 2 - arrow -->
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <Grid x:Name="PART_Shape" 
                              Grid.Column="1"
                              Grid.ColumnSpan="2" 
                              Margin="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="{StaticResource NeedleGrid00Column1}" />
                                <ColumnDefinition Width="{StaticResource NeedleGrid00Column2}" />
                             <ColumnDefinition Width="{StaticResource NeedleGrid00Column3}" />
                         </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="{StaticResource NeedleGrid00Row1}" />
                                <RowDefinition Height="{StaticResource NeedleGrid00Row2}" />
                                <RowDefinition Height="{StaticResource NeedleGrid00Row3}" />
                             <RowDefinition Height="{StaticResource NeedleGrid00Row4}" />
                             <RowDefinition Height="{StaticResource NeedleGrid00Row5}" />
                          </Grid.RowDefinitions>
                            
                            <Path Grid.Row="1"
                                     Grid.Column="1"
                                     Grid.RowSpan="3"
                                     Stretch="Fill"
                                     Data="M2.5,2.5 L4.5,27.563 2.5,52.5 z"
                                     Fill="{TemplateBinding ArrowBorderBrush}" />
                            
                            <Path Grid.Row="2"
                                  Grid.Column="1"
                                  Stretch="Fill" 
                                  Data="M1,1 L2,34.5 1,68 z" 
                                  StrokeThickness="2"
                                  Stroke="{TemplateBinding ArrowBackground}"
                                  Fill="{TemplateBinding ArrowBackground}" />
                        </Grid>

                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="{StaticResource NeedleGrid01Row1}" />
                                <RowDefinition Height="{StaticResource NeedleGrid01Row2}" />
                                <RowDefinition Height="{StaticResource NeedleGrid01Row3}" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="{StaticResource NeedleGrid01Column1}" />
                                <ColumnDefinition Width="{StaticResource NeedleGrid01Column2}" />
                                <ColumnDefinition Width="{StaticResource NeedleGrid01Column3}" />
                            </Grid.ColumnDefinitions>
                            <Ellipse Grid.Column="1"
                                     Grid.Row="1"
                                     Stretch="Uniform"
                                     Stroke="{TemplateBinding BorderBrush}"
                                     StrokeThickness="{StaticResource NeedleEllipseStrokeThickness}"
                                     Fill="{TemplateBinding Background}">
                            </Ellipse>
                        </Grid> 
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

