				</vsm:VisualStateGroup>
			</vsm:VisualStateManager.VisualStateGroups>
			

			<Border x:Name="Border" BorderThickness="1"
					CornerRadius="{StaticResource ControlOuterBorder_CornerRadius}"
					BorderBrush="{StaticResource ControlOuterBorderVertical_Normal}"
					Background="{StaticResource  ControlBackgroundVertical_Normal}">
				<Border x:Name="InnerBorder" BorderThickness="1"
						CornerRadius="{StaticResource ControlInnerBorder_CornerRadius}"
						BorderBrush="{StaticResource ControlInnerBorderVertical_Normal}" />
			</Border>
		</Grid>
		
	</ControlTemplate>

	<!--Vertical Range Selection Thumb Template-->
	<ControlTemplate x:Key="VerticalRangeSelectionThumbTemplate" TargetType="Thumb">
		<Grid>
			
			<vsm:VisualStateManager.VisualStateGroups>
				<vsm:VisualStateGroup x:Name="CommonStates">
					<vsm:VisualState x:Name="Normal" />
					<vsm:VisualState x:Name="MouseOver">
						<Storyboard>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="border"
									Storyboard.TargetProperty="Background" Duration="0:0:0">
								<DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource SliderRangeMiddleThumb_BackgroundVerical_State}" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</vsm:VisualState>
					<vsm:VisualState x:Name="Pressed">
						<Storyboard>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="border"
									Storyboard.TargetProperty="Background" Duration="0:0:0">
								<DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource SliderRangeMiddleThumb_BackgroundVerical_State}" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</vsm:VisualState>
					<vsm:VisualState x:Name="Disabled">
						<Storyboard>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="border"
									Storyboard.TargetProperty="Background" Duration="0:0:0">
								<DiscreteObjectKeyFrame KeyTime="0:0:0"
										Value="{StaticResource SliderRangeMiddleThumb_BackgroundVerical_Disable}" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</vsm:VisualState>
				</vsm:VisualStateGroup>
			</vsm:VisualStateManager.VisualStateGroups>
			
            <Border x:Name="border" Width="4" Opacity="1" BorderBrush="Transparent" 
                    BorderThickness="{StaticResource SliderRangeMiddleThumb_MarginVerical}"
					Background="{StaticResource SliderRangeMiddleThumb_BackgroundVerical}" />
		</Grid>
		
	</ControlTemplate>

	<!--TickBar-->
	<ControlTemplate x:Key="TickBarTemplate" TargetType="telerik:RadTickBar">
		<Canvas x:Name="LayoutRoot" Background="Transparent" />
	</ControlTemplate>

	<!--RadSlider-->
	<Style TargetType="telerik:RadSlider"
		    >
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="Orientation" Value="Horizontal" />
		<Setter Property="IsEnabled" Value="True" />
		<Setter Property="HandlesVisibility" Value="Collapsed" />
		<Setter Property="IsMouseWheelEnabled" Value="True" />
		<Setter Property="Background" Value="{StaticResource SliderBackgroundTrack}" />
		<Setter Property="BorderBrush" Value="{StaticResource SliderOuterBorderTrack}" />
		<Setter Property="Delay" Value="250" />
		<Setter Property="RepeatInterval" Value="250" />
		<Setter Property="StepAction" Value="ChangeRange" />
		<Setter Property="ThumbVisibility" Value="Visible" />
		<Setter Property="SelectionStart" Value="0.4" />
		<Setter Property="SelectionEnd" Value="0.6" />
		<Setter Property="MinimumRangeSpan" Value="0.0" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerik:RadSlider">
					<Grid x:Name="LayoutRoot">
						<Grid.Resources>
							<!-- Tick Template -->
							<DataTemplate x:Key="HorizontalTickTemplate">
								<Grid x:Name="RootElement" ToolTipService.ToolTip="{Binding}">
									<Rectangle Width="1" Height="5"
											Fill="{StaticResource SliderBackground_Tick}" />
								</Grid>
							</DataTemplate>
							<DataTemplate x:Key="VerticalTickTemplate">
								<Grid x:Name="RootElement" ToolTipService.ToolTip="{Binding}">
									<Rectangle Width="5" Height="1"
											Fill="{StaticResource SliderBackground_Tick}" />
								</Grid>
							</DataTemplate>
						</Grid.Resources>
						
						<vsm:VisualStateManager.VisualStateGroups>
							<VisualStateGroup x:Name="FocusStates">
								<VisualState x:Name="Focused">
									<Storyboard>
										<DoubleAnimation Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="FocusVisual" To="1"
												Duration="0" />
									</Storyboard>
								</VisualState>
								<VisualState x:Name="Unfocused">
									<Storyboard>
										<DoubleAnimation Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="FocusVisual" To="0"
												Duration="0" />
									</Storyboard>
								</VisualState>
							</VisualStateGroup>
							<!-- input valid states -->
							<vsm:VisualStateGroup x:Name="ValidationStates">
								<vsm:VisualState x:Name="Valid" />
								<vsm:VisualState x:Name="InvalidUnfocused">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ValidationErrorElement"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="InvalidFocused">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ValidationErrorElement"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
										</ObjectAnimationUsingKeyFrames>

										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="validationTooltip"
												Storyboard.TargetProperty="IsOpen">
											<DiscreteObjectKeyFrame KeyTime="0:0:0.1">
												<DiscreteObjectKeyFrame.Value>
													<sys:Boolean>True</sys:Boolean>
												</DiscreteObjectKeyFrame.Value>
											</DiscreteObjectKeyFrame>
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
						
						<Grid>

							<!--HorizontalTemplate-->
							<Grid x:Name="HorizontalTemplate">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
									<ColumnDefinition Width="Auto" />
								</Grid.ColumnDefinitions>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto" />
									<RowDefinition Height="Auto" />
									<RowDefinition Height="Auto" />
								</Grid.RowDefinitions>

								<!--[Horizontal] [TopTicks]-->
								<telerik:RadTickBar x:Name="TopTickBar" IsTabStop="False"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										Template="{StaticResource TickBarTemplate}" MinHeight="6"
										Placement="Top" Grid.Column="1"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />
								<!--[Horizontal] [BottomTicks]-->
								<telerik:RadTickBar x:Name="BottomTickBar" IsTabStop="False"
										Template="{StaticResource TickBarTemplate}" MinHeight="6"
										Placement="Bottom" Grid.Column="1" Grid.Row="2"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />

								<!--Track-->
								<Border VerticalAlignment="Center" Height="6" x:Name="Track"
										CornerRadius="{StaticResource SliderOuterBorderTrack_CornerRadius}"
										BorderThickness="{StaticResource SliderOuterBorderTrack_BorderThickness}"
										BorderBrush="{TemplateBinding BorderBrush}" Grid.Column="1"
										Grid.Row="1">
									<Border
											CornerRadius="{StaticResource SliderInnerBorderTrack_CornerRadius}"
											BorderThickness="{StaticResource SliderInnerBorderTrack_BorderThickness}"
											BorderBrush="{StaticResource SliderInnerBorderTrack}"
											Background="{TemplateBinding Background}" />
								</Border>

								<!--[Horizontal] [SingleThumbTemplate]-->
								<Grid x:Name="HorizontalSingleThumbTemplate" Grid.Row="1"
										Grid.Column="1" Height="18">
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="Auto" />
										<ColumnDefinition Width="Auto" />
										<ColumnDefinition Width="*" />
									</Grid.ColumnDefinitions>
									<Rectangle x:Name="HorizontalSingleLargeDecrease" Cursor="Hand"
											Opacity="0" Fill="Transparent"
											VerticalAlignment="Stretch" />
									<Thumb x:Name="HorizontalSingleThumb"
											IsEnabled="{TemplateBinding IsEnabled}" Width="8"
											Height="18" Grid.Column="1"
											Template="{StaticResource HorizontalThumbTemplate}"
											VerticalAlignment="Center"
											Visibility="{TemplateBinding ThumbVisibility}" />
									<Rectangle x:Name="HorizontalSingleLargeIncrease" Cursor="Hand"
											Grid.Column="2" Opacity="0" Fill="Transparent"
											VerticalAlignment="Stretch" />
								</Grid>

								<!--[Horizontal] [RangeSelectionTemplate]-->
								<Grid x:Name="HorizontalRangeTemplate" Grid.Row="1" Grid.Column="1"
										Height="18">
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="Auto" />
										<ColumnDefinition Width="Auto" />
										<ColumnDefinition Width="*" />
										<ColumnDefinition Width="Auto" />
										<ColumnDefinition Width="Auto" />
									</Grid.ColumnDefinitions>

									<Rectangle x:Name="HorizontalRangeLargeDecrease" Cursor="Hand"
											Opacity="0" Fill="Transparent" Grid.Column="0" />
									<Thumb x:Name="HorizontalRangeStartThumb"
											IsEnabled="{TemplateBinding IsEnabled}" Width="8"
											Height="18" Grid.Column="1"
											Template="{StaticResource HorizontalThumbTemplate}"
											VerticalAlignment="Center"
											Visibility="{TemplateBinding ThumbVisibility}" />
									<Thumb x:Name="HorizontalRangeMiddleThumb"
											IsEnabled="{TemplateBinding IsEnabled}" Grid.Column="2"
											Template="{StaticResource HorizontalRangeSelectionThumbTemplate}"
											VerticalAlignment="Center" />
									<Thumb x:Name="HorizontalRangeEndThumb"
											IsEnabled="{TemplateBinding IsEnabled}" Width="8"
											Height="18" Grid.Column="3"
											Template="{StaticResource HorizontalThumbTemplate}"
											VerticalAlignment="Center"
											Visibility="{TemplateBinding ThumbVisibility}" />
									<Rectangle x:Name="HorizontalRangeLargeIncrease" Cursor="Hand"
											Opacity="0" Fill="Transparent" Grid.Column="4" />
								</Grid>

								<!--[Horizontal] [DecreaseHandle]-->
								<RepeatButton x:Name="HorizontalDecreaseHandle" Grid.Row="1"
										IsEnabled="{TemplateBinding IsEnabled}"
										Delay="{TemplateBinding Delay}"
										Interval="{TemplateBinding RepeatInterval}"
										Visibility="{TemplateBinding HandlesVisibility}"
										Style="{StaticResource HandleStyle}" Margin="0 0 2 0" />

								<!--[Horizontal] [IncreaseHandle]-->
								<RepeatButton x:Name="HorizontalIncreaseHandle" Grid.Row="1"
										Grid.Column="2" IsEnabled="{TemplateBinding IsEnabled}"
										Delay="{TemplateBinding Delay}"
										Interval="{TemplateBinding RepeatInterval}"
										Visibility="{TemplateBinding HandlesVisibility}"
										RenderTransformOrigin="0.5,0.5" Margin="2 0 0 0"
										Style="{StaticResource HandleStyle}">
									<RepeatButton.RenderTransform>
										<ScaleTransform ScaleX="-1" ScaleY="1" />
									</RepeatButton.RenderTransform>
								</RepeatButton>
							</Grid>
							<!--VerticalTemplate-->
							<Grid x:Name="VerticalTemplate">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="Auto" />
								</Grid.ColumnDefinitions>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto" />
									<RowDefinition Height="*" />
									<RowDefinition Height="Auto" />
								</Grid.RowDefinitions>

								<!--[Vertical] [LeftTicks]-->
								<telerik:RadTickBar x:Name="LeftTickBar" IsTabStop="False"
										Template="{StaticResource TickBarTemplate}" MinWidth="6"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										Placement="Left" Grid.Column="0" Grid.Row="1"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />
								<!--[Vertical] [RightTicks]-->
								<telerik:RadTickBar x:Name="RightTickBar" IsTabStop="False"
										Template="{StaticResource TickBarTemplate}" MinWidth="6"
										Placement="Right" Grid.Column="2" Grid.Row="1"
										EnableSideTicks="{TemplateBinding EnableSideTicks}"
										TickFrequency="{TemplateBinding TickFrequency}"
										Ticks="{TemplateBinding Ticks}"
										IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
										Maximum="{TemplateBinding Maximum}"
										Minimum="{TemplateBinding Minimum}"
										TickTemplate="{TemplateBinding TickTemplate}"
										TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
										Orientation="{TemplateBinding Orientation}" />

								<!--Track-->
								<Border Width="6" HorizontalAlignment="Center"
										CornerRadius="{StaticResource SliderOuterBorderTrack_CornerRadius}"
										BorderThickness="{StaticResource SliderOuterBorderTrack_BorderThicknessVertical}"
										BorderBrush="{TemplateBinding BorderBrush}" Grid.Column="1"
										Grid.Row="1">
									<Border
											CornerRadius="{StaticResource SliderInnerBorderTrack_CornerRadius}"
											BorderThickness="{StaticResource SliderInnerBorderTrack_BorderThickness}"
											BorderBrush="{StaticResource SliderInnerBorderTrack}"
											Background="{TemplateBinding Background}" />
								</Border>

								<!--[Vertical] [TrackBackground]-->

								<!--[Vertical] [SingleThumbTemplate]-->
								<Grid x:Name="VerticalSingleThumbTemplate" Grid.Row="1"
										Grid.Column="1" Width="18">
									<Grid.RowDefinitions>
										<RowDefinition Height="*" />
										<RowDefinition Height="Auto" />
										<RowDefinition Height="Auto" />
									</Grid.RowDefinitions>
									<Rectangle x:Name="VerticalSingleLargeDecrease" Cursor="Hand"
											Grid.Row="2" Opacity="0"
											Fill="{StaticResource VerticalSingleLargeFill}"
											HorizontalAlignment="Stretch" />
									<Thumb x:Name="VerticalSingleThumb"
											IsEnabled="{TemplateBinding IsEnabled}" Width="18"
											Height="8" Grid.Row="1"
											Template="{StaticResource VerticalThumbTemplate}"
											HorizontalAlignment="Right"
											Visibility="{TemplateBinding ThumbVisibility}" />
									<Rectangle x:Name="VerticalSingleLargeIncrease" Cursor="Hand"
											Grid.Row="0" Opacity="0"
											Fill="{StaticResource VerticalSingleLargeFill}"
											HorizontalAlignment="Stretch" />
								</Grid>

								<!--[Vertical] [RangeSelectionTemplate]-->
								<Grid x:Name="VerticalRangeTemplate" Grid.Row="1" Grid.Column="1"
										Width="18">
									<Grid.RowDefinitions>
										<RowDefinition Height="Auto" />
										<RowDefinition Height="Auto" />
										<RowDefinition Height="*" />
										<RowDefinition Height="Auto" />
										<RowDefinition Height="Auto" />
									</Grid.RowDefinitions>

									<Rectangle x:Name="VerticalRangeLargeDecrease" Cursor="Hand"
											Opacity="0" Fill="Transparent" Grid.Row="4" />
									<Thumb x:Name="VerticalRangeStartThumb"
											IsEnabled="{TemplateBinding IsEnabled}" Height="8"
											Width="18" Grid.Row="3"
											Template="{StaticResource VerticalThumbTemplate}"
											HorizontalAlignment="Center"
											Visibility="{TemplateBinding ThumbVisibility}" />
									<Thumb x:Name="VerticalRangeMiddleThumb"
											IsEnabled="{TemplateBinding IsEnabled}" Grid.Row="2"
											Template="{StaticResource VerticalRangeSelectionThumbTemplate}"
											HorizontalAlignment="Center" />
									<Thumb x:Name="VerticalRangeEndThumb"
											IsEnabled="{TemplateBinding IsEnabled}" Height="8"
											Width="18" Grid.Row="1"
											Template="{StaticResource VerticalThumbTemplate}"
											HorizontalAlignment="Center"
											Visibility="{TemplateBinding ThumbVisibility}" />
									<Rectangle x:Name="VerticalRangeLargeIncrease" Cursor="Hand"
											Opacity="0" Fill="Transparent" Grid.Row="0" />
								</Grid>

								<!--[Vertical] [DecreaseHandle]-->
								<RepeatButton x:Name="VerticalDecreaseHandle"
										IsEnabled="{TemplateBinding IsEnabled}"
										Delay="{TemplateBinding Delay}"
										Interval="{TemplateBinding RepeatInterval}"
										HorizontalAlignment="Center" Grid.Column="1" Grid.Row="2"
										Visibility="{TemplateBinding HandlesVisibility}"
										Style="{StaticResource HandleStyle}" Margin="0 2 0 0"
										RenderTransformOrigin="0.5,0.5">
									<RepeatButton.RenderTransform>
										<TransformGroup>
											<ScaleTransform ScaleX="-1" ScaleY="-1" />
											<RotateTransform Angle="90" />
										</TransformGroup>
									</RepeatButton.RenderTransform>
								</RepeatButton>

								<!--[Vertical] [IncreaseHandle]-->
								<RepeatButton x:Name="VerticalIncreaseHandle"
										IsEnabled="{TemplateBinding IsEnabled}"
										Delay="{TemplateBinding Delay}"
										Interval="{TemplateBinding RepeatInterval}"
										HorizontalAlignment="Center" Grid.Column="1" Grid.Row="0"
										Visibility="{TemplateBinding HandlesVisibility}"
										RenderTransformOrigin="0.5,0.5"
										Style="{StaticResource HandleStyle}" Margin="0,0,0,2">
									<RepeatButton.RenderTransform>
										<TransformGroup>
											<ScaleTransform ScaleX="1" ScaleY="-1" />
											<RotateTransform Angle="90" />
										</TransformGroup>
									</RepeatButton.RenderTransform>
								</RepeatButton>
							</Grid>

						</Grid>
						<Rectangle x:Name="FocusVisual" Stroke="{StaticResource FocusBrushBlack}"
								Opacity="0" StrokeDashArray="1 2" StrokeThickness="1" RadiusX="2"
								RadiusY="2" />
						<!--Error Visual Element-->
						<Border x:Name="ValidationErrorElement" BorderThickness="1" CornerRadius="1"
								BorderBrush="#FFDB000C" Visibility="Collapsed">
							<ToolTipService.ToolTip>
								<ToolTip x:Name="validationTooltip"
										Template="{StaticResource ValidationToolTipTemplate}"
										Placement="Right"
										PlacementTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
										DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}}">
									<ToolTip.Triggers>
										<EventTrigger RoutedEvent="Canvas.Loaded">
											<EventTrigger.Actions>
												<BeginStoryboard>
													<Storyboard>
														<ObjectAnimationUsingKeyFrames
																Storyboard.TargetName="validationTooltip"
																Storyboard.TargetProperty="IsHitTestVisible">
															<DiscreteObjectKeyFrame KeyTime="0">
																<DiscreteObjectKeyFrame.Value>
																	<sys:Boolean>true</sys:Boolean>
																</DiscreteObjectKeyFrame.Value>
															</DiscreteObjectKeyFrame>
														</ObjectAnimationUsingKeyFrames>
													</Storyboard>
												</BeginStoryboard>
											</EventTrigger.Actions>
										</EventTrigger>
									</ToolTip.Triggers>
								</ToolTip>
							</ToolTipService.ToolTip>
							<Grid Width="12" Height="12" HorizontalAlignment="Right"
									Margin="1,-4,-4,0" VerticalAlignment="Top"
									Background="Transparent">
								<Path Margin="1,3,0,0" Data="M 1,0 L6,0 A 2,2 90 0 1 8,2 L8,7 z"
										Fill="#FFDC000C" />
								<Path Margin="1,3,0,0" Data="M 0,0 L2,0 L 8,6 L8,8" Fill="#ffffff" />
							</Grid>
						</Border>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
</ResourceDictionary>