                                    <telerikChromes:ButtonChrome x:Name="DropDownPartChrome"
                                            telerik:StyleManager.Theme="{StaticResource Theme}" CornerRadius="0 1 1 0"
                                            RenderEnabled="{TemplateBinding IsEnabled}" RenderNormal="False"
                                            RenderActive="{TemplateBinding IsMouseOver}"
                                            RenderChecked="{TemplateBinding IsOpen}"
                                            RenderMouseOver="{Binding IsMouseOver, ElementName=DropDownPart}"
                                            RenderPressed="{Binding IsPressed, ElementName=DropDownPart}" />
                                    <Path x:Name="DropDownIndicator" IsHitTestVisible="False"
                                            Fill="{StaticResource ControlElement_Normal}" HorizontalAlignment="Center"
                                            VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Margin="5,3,5,3"
                                            Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z"
                                             />
                                </Grid>
                            </telerik:RadToggleButton>

                            <telerik:RadButton x:Name="ButtonPart" Grid.Row="1" Grid.Column="1"
                                    HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    Padding="{TemplateBinding Padding}"
                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}">
                                <telerik:RadButton.Template>
                                    <ControlTemplate TargetType="telerik:RadButton">
                                        <ContentPresenter />
                                    </ControlTemplate>
                                </telerik:RadButton.Template>
                                <Grid>
                                    <telerikChromes:ButtonChrome x:Name="ButtonPartChrome"
                                            telerik:StyleManager.Theme="{StaticResource Theme}" CornerRadius="1 0 0 1"
                                            RenderEnabled="{TemplateBinding IsEnabled}" RenderNormal="False"
                                            RenderActive="{TemplateBinding IsMouseOver}"
                                            RenderHighlighted="{TemplateBinding IsOpen}" RenderChecked="False"
                                            RenderMouseOver="{Binding IsMouseOver, ElementName=ButtonPart}"
                                            RenderPressed="{Binding IsPressed, ElementName=ButtonPart}" />
                                    <ContentPresenter x:Name="Content" Margin="{TemplateBinding Padding}"
                                            Content="{TemplateBinding Content}"
                                            ContentTemplate="{TemplateBinding ContentTemplate}"
                                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                 />
                                </Grid>
                            </telerik:RadButton>
                        </Grid>
                        

                            
                            <telerikPrimitives:Popup x:Name="DropDownPopup">
                                
                                <Grid>
                                    <Border x:Name="DropDownPopupBorder" BorderThickness="1"
                                            BorderBrush="{StaticResource ControlOuterBorder}"
                                            Background="{StaticResource ControlBackground}"
                                            CornerRadius="{TemplateBinding CornerRadius}" Margin="0,1,4,4" MinWidth="3"
                                            MinHeight="3" Width="{TemplateBinding DropDownWidth}"
                                            Height="{TemplateBinding DropDownHeight}"
                                            MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                            MaxHeight="{TemplateBinding DropDownMaxHeight}">
                                        <ContentPresenter x:Name="DropDownPopupContent"
                                                Content="{TemplateBinding DropDownContent}"
                                                ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                                HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                                                DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
                                    </Border>
                                </Grid>
                                
                            </telerikPrimitives:Popup>
                            
                            
                    </Grid>

                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate TargetType="ToggleButton" x:Key="ToolBarDropdownButtonH">
        <Grid Background="Transparent">
            
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="Normal" />
                    <vsm:VisualState x:Name="Disabled">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                    Storyboard.TargetName="OrnamentGrid">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="0.25" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                    Storyboard.TargetName="MouseOverDecorator">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="1" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                    Storyboard.TargetName="backgroundDecoratorCheck">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="1" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
                <vsm:VisualStateGroup x:Name="CheckStates">
                    <vsm:VisualState x:Name="Unchecked" />
                    <vsm:VisualState x:Name="Checked">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                    Storyboard.TargetName="backgroundDecoratorCheck">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="1" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>
            
            <Border BorderThickness="1" BorderBrush="{StaticResource ToolBar_ToggleButton_normalOuterBorder}"
                    Background="{StaticResource ToolBar_ToggleButton_normalBackground}" x:Name="backgroundDecorator"
                    CornerRadius="0 3 3 0">
                <Border BorderThickness="{StaticResource ToolBar_ToggleButton_InnerBorderThickness}"
                        CornerRadius="0 2 2 0" BorderBrush="{StaticResource ToolBar_ToggleButton_normalInnerBorder}" />
            </Border>

            <Border BorderThickness="1" BorderBrush="{StaticResource ControlOuterBorder_MouseOver}"
                    Background="{StaticResource ControlBackground_MouseOver}" x:Name="MouseOverDecorator"
                    CornerRadius="0 3 3 0" Opacity="0">
                <Border BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="0 2 2 0"
                        BorderBrush="{StaticResource ControlInnerBorder_MouseOver}" />
            </Border>

            <Border BorderThickness="1" BorderBrush="{StaticResource ControlOuterBorder_Pressed}"
                    Background="{StaticResource ControlBackground_Pressed}" x:Name="backgroundDecoratorCheck"
                    CornerRadius="0 3 3 0" Opacity="0">
                <Border BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="0 2 2 0"
                        BorderBrush="{StaticResource ControlInnerBorder_Pressed}" />
            </Border>
            <Grid x:Name="OrnamentGrid" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0 0 3 3"
                    Opacity="1">
                <Path Fill="#FFFFFFFF" Stretch="Fill" Margin="2,5,0,0" HorizontalAlignment="Center"
                        VerticalAlignment="Top" Width="5" Height="3"
                        Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z" />
                <Path Fill="{StaticResource ControlElement_Normal}" Stretch="Fill" Margin="0,4,0,0"
                        HorizontalAlignment="Center" VerticalAlignment="Top" Width="5" Height="3"
                        Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z" />
                <Path Fill="#FFFFFFFF" Stretch="Fill" Margin="2,1,0,0" HorizontalAlignment="Center"
                        VerticalAlignment="Top" Width="5" Height="1" Data="M0,0 L5,0 5,1 0,1 0,0 z" />
                <Path Fill="{StaticResource ControlElement_Normal}" Stretch="Fill" Margin="0,0,0,0"
                        HorizontalAlignment="Center" VerticalAlignment="Top" Width="5" Height="1"
                        Data="M0,0 L5,0 5,1 0,1 0,0 z" />
            </Grid>
        </Grid>
        
    </ControlTemplate>

    <telerikNavigation:ToolBarContainerStyleSelector x:Key="ToolBarContainerStyleSelector">
        <telerikNavigation:ToolBarContainerStyle TypeName="RadToolBarSeparator"
                ContainerStyle="{StaticResource ToolBarSeparator}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="TextBlock" ContainerStyle="{StaticResource ToolBarTextBlock}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="TextBox" ContainerStyle="{StaticResource ToolBarTextBox}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="Button" ContainerStyle="{StaticResource ToolBarButton}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="ToggleButton"
                ContainerStyle="{StaticResource ToolBarToggleButton}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="CheckBox" ContainerStyle="{StaticResource ToolBarCheckBox}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="RadioButton"
                ContainerStyle="{StaticResource ToolBarRadioButton}" />

        <telerikNavigation:ToolBarContainerStyle TypeName="RadButton" ContainerStyle="{StaticResource ToolBarRadButton}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="RadToggleButton"
                ContainerStyle="{StaticResource ToolBarRadToggleButton}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="RadRadioButton"
                ContainerStyle="{StaticResource ToolBarRadRadioButton}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="RadDropDownButton"
                ContainerStyle="{StaticResource ToolBarRadDropDownButton}" />
        <telerikNavigation:ToolBarContainerStyle TypeName="RadSplitButton"
                ContainerStyle="{StaticResource ToolBarRadSplitButton}" />
    </telerikNavigation:ToolBarContainerStyleSelector>

    <Style TargetType="telerikNavigation:RadToolBar"
         >
        <Setter Property="ItemContainerStyleSelector" Value="{StaticResource ToolBarContainerStyleSelector}" />
        <Setter Property="MinWidth" Value="25" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBar">
                    <Grid MinHeight="24" x:Name="RootElement"  >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="c0" Width="Auto" />
                            <ColumnDefinition x:Name="c1" Width="*" />
                            <ColumnDefinition x:Name="c2" Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="r0" Height="Auto" />
                            <RowDefinition x:Name="r1" Height="*" />
                            <RowDefinition x:Name="r2" Height="Auto" />
                        </Grid.RowDefinitions>
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStateGroup">
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                Storyboard.TargetName="RootElement" To="0.5" Duration="0:0:0.2" />
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="OrientationStateGroup">
                                <vsm:VisualState x:Name="Horizontal">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="c1"
                                                Storyboard.TargetProperty="Width" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="*" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="r0"
                                                Storyboard.TargetProperty="Height" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="*" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="r1"
                                                Storyboard.TargetProperty="Height" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Auto" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="r2"
                                                Storyboard.TargetProperty="Height" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Auto" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                Storyboard.TargetProperty="Orientation" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Horizontal" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                Storyboard.TargetProperty="(Grid.Row)" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                Storyboard.TargetProperty="(Grid.Column)" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="1" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                Storyboard.TargetProperty="Margin" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0,1,4,1" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropdownButtonHolder"
                                                Storyboard.TargetProperty="(Grid.Row)" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropdownButtonHolder"
                                                Storyboard.TargetProperty="(Grid.Column)" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="2" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropdownButtonHolder"
                                                Storyboard.TargetProperty="Margin" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="-2 0 0 0" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="GripOrnamentH"
                                                Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="GripOrnamentV"
                                                Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBackground"
                                                Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                    Value="{StaticResource ToolBar_InnerBackground}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Vertical">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="c0"
                                                Storyboard.TargetProperty="Width" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="*" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="c1"
                                                Storyboard.TargetProperty="Width" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Auto" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="c2"
                                                Storyboard.TargetProperty="Width" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Auto" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="r1"
                                                Storyboard.TargetProperty="Height" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="*" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                Storyboard.TargetProperty="Orientation" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Vertical" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                Storyboard.TargetProperty="(Grid.Row)" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="1" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                Storyboard.TargetProperty="(Grid.Column)" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_StripPanel"
                                                Storyboard.TargetProperty="Margin" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="1,0,1,4" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropdownButtonHolder"
                                                Storyboard.TargetProperty="(Grid.Row)" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="2" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropdownButtonHolder"
                                                Storyboard.TargetProperty="(Grid.Column)" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropdownButtonHolder"
                                                Storyboard.TargetProperty="Margin" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0 -2 0 0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropdownButtonHolder"
                                                Storyboard.TargetProperty="LayoutTransform" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <TransformGroup>
                                                        <RotateTransform x:Name="ToolBarToggleButtonAngle" Angle="-90" />
                                                        <ScaleTransform x:Name="ToolBarToggleButtonScale" ScaleX="1"
                                                                ScaleY="-1" />
                                                    </TransformGroup>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="GripOrnamentH"
                                                Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="GripOrnamentV"
                                                Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>

                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBackground"
                                                Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                    Value="{StaticResource ToolBar_InnerBackgroundV}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>

                        <telerikPrimitives:LayoutTransformControl Grid.Column="2" x:Name="DropdownButtonHolder">
                            <telerikPrimitives:LayoutTransformControl.Content>
                                
                                <ToggleButton Visibility="{TemplateBinding OverflowButtonVisibility}"
                                        x:Name="PART_DropdownButton" Template="{StaticResource ToolBarDropdownButtonH}"
                                        IsEnabled="{TemplateBinding HasOverflowItems}"
                                        IsChecked="{TemplateBinding IsOverflowOpen}"
                                    
                                     Width="14" Margin="2 0 0 0" />
                            </telerikPrimitives:LayoutTransformControl.Content>
                        </telerikPrimitives:LayoutTransformControl>
                        
                        
                                
                                <Popup Grid.Column="2" x:Name="PART_Overflow">
                                    
                                    <Border Margin="0" MinWidth="55" MinHeight="33">
                                        <Border BorderBrush="{StaticResource OverflowBorderInnerBrush}"
                                                Background="{StaticResource OverflowContentBackgroundBrush}"
                                                BorderThickness="1" Padding="1,2,1,2">
                                            
                                                
                                                <StackPanel 
                                                    x:Name="PART_OverflowPanel"
                                            Orientation="Vertical" VerticalAlignment="Top" />
                                        </Border>
                                    </Border>
                                </Popup>
                                
                        <Border x:Name="InnerBackground" Grid.ColumnSpan="2" Grid.RowSpan="2"
                                BorderBrush="{StaticResource ToolBar_OuterBorder}" CornerRadius="3"
                                HorizontalAlignment="Stretch" BorderThickness="1"
                                Background="{StaticResource ToolBar_InnerBackground}">
                            <Border BorderThickness="1" CornerRadius="2"
                                    BorderBrush="{StaticResource ToolBar_InnerBorder}">
                            </Border>
                        </Border>

                        <Border x:Name="BindingBackground" Grid.ColumnSpan="2" Grid.RowSpan="2"
                                BorderBrush="{TemplateBinding BorderBrush}" CornerRadius="3"
                                HorizontalAlignment="Stretch" BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}">
                        </Border>
                        <Grid Visibility="{TemplateBinding GripVisibility}">
                            <Grid x:Name="GripOrnamentH" Visibility="Visible">
                                <Path Fill="{StaticResource ToolBar_OuterGripOrnament}" Stretch="Fill" Margin="5,1,0,0"
                                        Width="2" Height="14" VerticalAlignment="Center" HorizontalAlignment="Left"
                                        Data="M0,12 L2,12 2,14 0,14 0,12 z M0,8 L2,8 2,10 0,10 0,8 z M0,4 L2,4 2,6 0,6 0,4 z M0,0 L2,0 2,2 0,2 0,0 z" />
                                <Path Fill="{StaticResource ToolBar_InnerGripOrnament}" Stretch="Fill"
                                        Margin="4.4,0.4,0,0" Width="2" Height="14" VerticalAlignment="Center"
                                        HorizontalAlignment="Left"
                                        Data="M0,12 L2,12 2,14 0,14 0,12 z M0,8 L2,8 2,10 0,10 0,8 z M0,4 L2,4 2,6 0,6 0,4 z M0,0 L2,0 2,2 0,2 0,0 z" />
                            </Grid>
                            <Grid x:Name="GripOrnamentV" Visibility="Collapsed">
                                <Path Fill="{StaticResource ToolBar_OuterGripOrnament}" Stretch="Fill" Margin="1,5,0,0"
                                        Width="14" Height="2" VerticalAlignment="Top" HorizontalAlignment="Center"
                                        Data="M12,0 L12,2 14,2 14,0 12,0 z M8,0 L8,2 10,2 10,0 8,0 z M4,0 L4,2 6,2 6,0 4,0 z M0,0 L0,2 2,2 2,0 0,0 z" />
                                <Path Fill="{StaticResource ToolBar_InnerGripOrnament}" Stretch="Fill"
                                        Margin="0.4,4.4,0,0" Width="14" Height="2" VerticalAlignment="Top"
                                        HorizontalAlignment="Center"
                                        Data="M12,0 L12,2 14,2 14,0 12,0 z M8,0 L8,2 10,2 10,0 8,0 z M4,0 L4,2 6,2 6,0 4,0 z M0,0 L0,2 2,2 2,0 0,0 z" />
                            </Grid>
                        </Grid>
                        
                            
                            <StackPanel 
                        
                                x:Name="PART_StripPanel" Grid.Column="1" Margin="0,1,4,1"
                            Orientation="Horizontal" />
                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="telerikNavigation:RadToolBarTray"
         >
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikToolBar:RadToolBarTrayPanel x:Name="PART_BandPanel" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBarTray">
                    <Border Padding="{TemplateBinding Padding}" Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <ItemsPresenter x:Name="PART_ItemsPresenter" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    

</ResourceDictionary>
