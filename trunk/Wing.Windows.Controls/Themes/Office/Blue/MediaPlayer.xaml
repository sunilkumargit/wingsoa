    <!--Volume Slider Style-->
    <Style x:Key="VolumeSliderStyle" TargetType="telerik:RadSlider">
        <Setter Property="Orientation" Value="Horizontal" />
        <Setter Property="IsEnabled" Value="True" />
        <Setter Property="HandlesVisibility" Value="Collapsed" />
        <Setter Property="IsMouseWheelEnabled" Value="True" />
        <Setter Property="Maximum" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSlider">
                    <Grid x:Name="LayoutRoot">
                        <!--[Horizontal] [Template]-->
                        <Grid x:Name="HorizontalTemplate" MinHeight="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="23" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <!--[Horizontal] [TrackBackground]-->

                            <!--[Horizontal] [SingleThumbTemplate]-->
                            <Rectangle Height="8" HorizontalAlignment="Stretch" VerticalAlignment="Center" RadiusX="1"
                                    RadiusY="1" Width="Auto" Grid.Column="1" Grid.Row="1"
                                    Stroke="{StaticResource MediaPlayer_Slider_OuterBorderBrushThump}"
                                    Fill="{StaticResource MediaPlayer_Slider_OuterBackgroundThump}" />

                            <!--[Horizontal] [SingleThumbTemplate]-->
                            <Grid x:Name="HorizontalSingleThumbTemplate" Cursor="Hand" Grid.Row="1" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Height="6" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                                        RadiusX="0.5" RadiusY="0.5" Width="Auto" Grid.Column="0" Grid.ColumnSpan="2"
                                        Margin="1 0" Fill="{StaticResource MediaPlayer_Slider_InnerBackgroundThump}" />
                                <Rectangle x:Name="HorizontalSingleLargeDecrease" Fill="Transparent" />
                                <Thumb x:Name="HorizontalSingleThumb" Width="6" Height="6" Grid.Column="1"
                                        Template="{StaticResource TimeSliderThumbTemplate}" />
                                <Rectangle x:Name="HorizontalSingleLargeIncrease" Grid.Column="2" Fill="Transparent" />
                            </Grid>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--TickBar-->
    <ControlTemplate x:Key="TickBarTemplate" TargetType="telerik:RadTickBar">
        <Canvas x:Name="LayoutRoot" Background="Transparent" />
    </ControlTemplate>

    <!--TimeSlider Style-->
    <Style x:Key="TimeSliderStyle" TargetType="telerik:RadSlider">
        <Setter Property="Orientation" Value="Horizontal" />
        <Setter Property="IsEnabled" Value="True" />
        <Setter Property="HandlesVisibility" Value="Collapsed" />
        <Setter Property="IsMouseWheelEnabled" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSlider">
                    <Grid x:Name="LayoutRoot">
                        <Grid.Resources>
                            <!-- Tick Template -->
                            <DataTemplate x:Key="HorizontalTickTemplate">
                                <Grid Height="6" Margin="0 1 0 0" Width="1">
                                    <Border UseLayoutRounding="True"
                                            BorderBrush="{StaticResource MediaPlayer_Slider_HorizontalTimeTemplateBorder}"
                                            BorderThickness="1 0 0 0" />
                                </Grid>
                            </DataTemplate>
                        </Grid.Resources>
                        <!--[Horizontal] [Template]-->
                        <Grid x:Name="HorizontalTemplate" MinHeight="0" Height="8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <!--[Horizontal] [SingleThumbTemplate]-->
                            <Rectangle Height="6" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                                    Grid.Column="1" Grid.Row="1" Stroke="Transparent" Fill="Transparent" />

                            <!--[Horizontal] [SingleThumbTemplate]-->
                            <Grid x:Name="HorizontalSingleThumbTemplate" Grid.Row="1" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                <Rectangle Height="6" Cursor="Hand" Margin="1 0" VerticalAlignment="Center"
                                        RadiusX="0.5" RadiusY="0.5" Width="Auto" Grid.ColumnSpan="2"
                                        Fill="{StaticResource MediaPlayer_Slider_InnerBackgroundThump}" />

                                <telerik:RadTickBar x:Name="TopTickBar" Grid.ColumnSpan="3" VerticalAlignment="Top"
                                        Template="{StaticResource TickBarTemplate}" Height="8" Placement="Top"
                                        TickFrequency="{TemplateBinding TickFrequency}" Ticks="{TemplateBinding Ticks}"
                                        EnableSideTicks="{TemplateBinding EnableSideTicks}"
                                        IsDirectionReversed="{TemplateBinding IsDirectionReversed}"
                                        Maximum="{TemplateBinding Maximum}" Minimum="{TemplateBinding Minimum}"
                                        TickTemplate="{TemplateBinding TickTemplate}"
                                        TickTemplateSelector="{TemplateBinding TickTemplateSelector}"
                                        Orientation="{TemplateBinding Orientation}" />
                                <Rectangle x:Name="HorizontalSingleLargeDecrease" Fill="Transparent" />
                                <Thumb x:Name="HorizontalSingleThumb" Width="17" Height="8" Grid.Column="1"
                                        Template="{StaticResource TimeSliderThumbTemplate}" />
                                <Rectangle x:Name="HorizontalSingleLargeIncrease" Grid.Column="2" Fill="Transparent" />
                            </Grid>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate x:Key="MediaPlayer_Button" TargetType="telerik:RadButton">
        <Grid Height="22" Cursor="Hand" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="26">
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="Normal" />
                    <vsm:VisualState x:Name="MouseOver" />
                    <vsm:VisualState x:Name="Pressed" />
                    <vsm:VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="ContentPresenter"
                                    Storyboard.TargetProperty="Opacity">
                                <DiscreteDoubleKeyFrame KeyTime="0" Value="0.4" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>
            <Border x:Name="Border" CornerRadius="{StaticResource MediaPlayer_Button_OuterCornerRadius}"
                    BorderBrush="{StaticResource MediaPlayer_ButtonOuterBorder_Normal}"
                    BorderThickness="{StaticResource MediaPlayer_Button_BorderThickness}"
                    Background="{StaticResource MediaPlayer_ButtonOuterBackground_Normal}">
                <Border x:Name="InnerBorder" CornerRadius="{StaticResource MediaPlayer_Button_InnerCornerRadius}"
                        BorderBrush="{StaticResource MediaPlayer_ButtonInnerBorder_Normal}"
                        BorderThickness="{StaticResource MediaPlayer_Button_BorderThickness}" />
            </Border>

            <telerikChromes:ButtonChrome x:Name="ButtonVisual" telerik:StyleManager.Theme="{StaticResource Theme}"
                    CornerRadius="{StaticResource MediaPlayer_Button_OuterCornerRadius}" RenderNormal="False"
                    RenderEnabled="{TemplateBinding IsEnabled}" RenderMouseOver="{TemplateBinding IsMouseOver}"
                    RenderPressed="{TemplateBinding IsPressed}" />

            <ContentPresenter x:Name="ContentPresenter"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}" />
        </Grid>

    </ControlTemplate>

    <ControlTemplate x:Key="MediaPlayer_PlayButton" TargetType="telerik:RadButton">
        <Grid Height="118" Width="118" Cursor="Hand" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="Normal" />
                    <vsm:VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Path"
                                    Storyboard.TargetProperty="Stroke" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0"
                                        Value="{StaticResource MediaPlayer_PlayButtonPathBorderBrush_State}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Path" Storyboard.TargetProperty="Fill"
                                    Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0"
                                        Value="{StaticResource MediaPlayer_PlayButtonPath_State}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Path"
                                    Storyboard.TargetProperty="Stroke" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0"
                                        Value="{StaticResource MediaPlayer_PlayButtonPathBorderBrush_State}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Path" Storyboard.TargetProperty="Fill"
                                    Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0"
                                        Value="{StaticResource MediaPlayer_PlayButtonPath_State}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Button"
                                    Storyboard.TargetProperty="Opacity">
                                <DiscreteDoubleKeyFrame KeyTime="0" Value="0" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>

            <Grid x:Name="Button">
                <Border BorderThickness="{StaticResource MediaPlayer_PlayButton_OuterBorderThickness}"
                        Grid.ColumnSpan="2" BorderBrush="{StaticResource MediaPlayer_PlayButton_OuterBorderBrush}"
                        CornerRadius="{StaticResource MediaPlayer_PlayButton_OuterCornerRadius}"
                        Background="{StaticResource MediaPlayer_PlayButton_OuterBackground}">
                    <Border BorderThickness="{StaticResource MediaPlayer_PlayButton_InnerBorderThickness}"
                            CornerRadius="{StaticResource MediaPlayer_PlayButton_InnerCornerRadius}"
                            BorderBrush="{StaticResource MediaPlayer_PlayButton_InnerBorderBrush}" />
                </Border>

                <Path x:Name="Path" Stretch="Fill" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        Stroke="{StaticResource MediaPlayer_PlayButtonPathBorderBrush_Normal}" Width="62"
                        Fill="{StaticResource MediaPlayer_PlayButtonPath_Normal}" Height="62"
                        Data="M0,0 L12,5.937115 0,12 z" />

                <ContentPresenter x:Name="ContentPresenter"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" />
            </Grid>
        </Grid>

    </ControlTemplate>

    <Style TargetType="player:RadMediaPlayer">
        <Setter Property="FontSize" Value="10" />
        <Setter Property="Foreground" Value="{StaticResource MediaPlayer_ForegroundTime}" />
        <Setter Property="Volume" Value="0.5" />
        <Setter Property="Background" Value="Black" />
        <Setter Property="Padding" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="player:RadMediaPlayer">
                    <Grid x:Name="LayoutRoot" Background="{TemplateBinding Background}"
                            Margin="{TemplateBinding Padding}">
                        <Grid.Triggers>
                            <EventTrigger RoutedEvent="Grid.Loaded">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation RepeatBehavior="Forever" From="0" To="360" Duration="0:0:0.7"
                                                Storyboard.TargetName="ellipse"
                                                Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </Grid.Triggers>

                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualState x:Name="Normal">
                                    <Storyboard Duration="0">
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DisabledVisual"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Disabled">
                                    <Storyboard Duration="0">
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DisabledVisual"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>

                            <vsm:VisualStateGroup x:Name="ErrorStates">
                                <vsm:VisualState x:Name="Error">
                                    <Storyboard Duration="0">
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ErrorElement"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="NoError">
                                    <Storyboard Duration="0">
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ErrorElement"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>

                            <vsm:VisualStateGroup x:Name="VideoControlsStates">
                                <vsm:VisualState x:Name="VisibleVideoControls">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BottomPart"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation Storyboard.TargetName="BottomPart"
                                                Storyboard.TargetProperty="Opacity" To="1" Duration="00:00:.7" />
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="HiddenVideoControls">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BottomPart"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.7" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation Storyboard.TargetName="BottomPart"
                                                Storyboard.TargetProperty="Opacity" To="0" Duration="00:00:.7" />
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="PlaylistStates">
                                <vsm:VisualState x:Name="HiddenPlaylist">
                                    <Storyboard>
                                        <!--<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="Playlist"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0:0:0.2"
													Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>-->
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="Playlist"
                                                Storyboard.TargetProperty="Opacity" To="0" />
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PlayButton"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.2" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="VisiblePlaylist">
                                    <Storyboard>
                                        <!--<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="Playlist"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
										</ObjectAnimationUsingKeyFrames>-->
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="Playlist"
                                                Storyboard.TargetProperty="Opacity" To="1" />
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PlayButton"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.2" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="ChaptersPresenterStates">
                                <vsm:VisualState x:Name="HiddenChaptersPresenter">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ChaptersPresenter"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="00:00:01" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
                                                Storyboard.TargetName="ChaptersPresenterBackground"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="00:00:01" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="VisibleChaptersPresenter">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ChaptersPresenter"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
                                                Storyboard.TargetName="ChaptersPresenterBackground"
                                                Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="BufferingStates">
                                <vsm:VisualState x:Name="VisibleLoading">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LoadingElement"
                                                Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="HiddenLoading">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LoadingElement"
                                                Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>

                        <MediaElement x:Name="MediaElement" AutoPlay="True"
                                DataContext="{TemplateBinding SelectedMediaItem}" Volume="{TemplateBinding Volume}"
                                Height="{Binding VideoHeight}" Width="{Binding VideoWidth}"
                                Stretch="{Binding VideoStretch}" BufferingTime="{Binding BufferingTime}" />

                        <!-- poster image -->
                        <Image x:Name="PosterImage" />

                        <!-- loading element -->
                        <Grid x:Name="LoadingElement" Visibility="Collapsed">
                            <Rectangle Fill="Black" Opacity="0.7" />
                            <Grid RenderTransformOrigin="0.5,0.5" x:Name="loadingElements" Width="40" Height="40">

                                <Ellipse Width="37" Height="37" Stroke="{StaticResource LoadingElementStroke}"
                                        Opacity="0.4" StrokeThickness="2" />

                                <Ellipse x:Name="ellipse" StrokeThickness="5" Width="40" Height="40"
                                        RenderTransformOrigin="0.5,0.5"
                                        Stroke="{StaticResource LoadingOuterElementStroke}">
                                    <Ellipse.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform />
                                            <SkewTransform />
                                            <RotateTransform />
                                            <TranslateTransform />
                                        </TransformGroup>
                                    </Ellipse.RenderTransform>
                                </Ellipse>
                            </Grid>
                        </Grid>

                        <!-- eror element -->
                        <Grid x:Name="ErrorElement" Height="36" VerticalAlignment="Center" HorizontalAlignment="Center"
                                Visibility="Collapsed">
                            <Rectangle Stroke="#FFAAAEB5" RadiusX="2" RadiusY="2" Fill="#FFFBFCFC" />
                            <Ellipse HorizontalAlignment="Left" Margin="8,6,0,5" Width="25">
                                <Ellipse.Stroke>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFAF3165" Offset="0" />
                                        <GradientStop Color="#FF911348" Offset="1" />
                                    </LinearGradientBrush>
                                </Ellipse.Stroke>
                                <Ellipse.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFDF6093" Offset="0.006" />
                                        <GradientStop Color="#FF8C0E43" Offset="1" />
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                            </Ellipse>
                            <Path Stretch="Fill" StrokeThickness="0" HorizontalAlignment="Left" Margin="14,13,0,12"
                                    Width="13" UseLayoutRounding="False"
                                    Data="M0,0 L3,0 L3,1 L4,1 L4,2 L5,2 L5,3 L6,3 L6,4 L7,4 L7,3 L8,3 L8,2 L9,2 L9,1 L10,1 L10,0 L13,0 L13,1 L12,1 L12,2 L11,2 L11,3 L10,3 L10,4 L9,4 L9,5 L8,5 L8,6 L9,6 L9,7 L10,7 L10,8 L11,8 L11,9 L12,9 L12,10 L13,10 L13,11 L10,11 L10,10 L9,10 L9,9 L8,9 L8,8 L7,8 L7,7 L6,7 L6,8 L5,8 L5,9 L4,9 L4,10 L3,10 L3,11 L0,11 L0,10 L1,10 L1,9 L2,9 L2,8 L3,8 L3,7 L4,7 L4,6 L5,6 L5,5 L4,5 L4,4 L3,4 L3,3 L2,3 L2,2 L1,2 L1,1 L0,1 z">
                                <Path.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="White" Offset="0" />
                                        <GradientStop Color="#FFC7D1D4" Offset="1" />
                                    </LinearGradientBrush>
                                </Path.Fill>
                            </Path>

                            <TextBlock Margin="46,7,6,10" FontSize="16" Foreground="#FF252525"
                                    Text="Failed to open media!" TextWrapping="Wrap" />
                        </Grid>

                        <!--Header content, Can be used for title, ads...-->
                        <Border x:Name="HeaderPresenter" Grid.ColumnSpan="3">
                            <ContentPresenter Content="{TemplateBinding Header}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                        </Border>

                        <!-- Playlist element. Do NOT collapse before Layout. -->
                        <Grid x:Name="Playlist" Opacity="0">
                            <Border Padding="10 10 5 10" Margin="{StaticResource PlayList_Margin}"
                                    CornerRadius="{StaticResource PlayList_CornerRadius}"
                                    Background="{StaticResource PlayList_Background}"
                                    BorderBrush="{StaticResource PlayList_Border}"
                                    BorderThickness="{StaticResource PlayList_BorderThickness}" />
                            <ScrollViewer Style="{StaticResource ScrollViewerPlayList}" Padding="0 17 9 21"
                                    Margin="10 5 15 53" BorderThickness="0" VerticalScrollBarVisibility="Auto">
                                <ItemsPresenter />
                            </ScrollViewer>
                        </Grid>

                        <telerik:RadButton x:Name="PlayButton" Command="player:MediaCommands.Play"
                                Template="{StaticResource MediaPlayer_PlayButton}" />
                        <!-- MediaPlayer elements -->
                        <Grid VerticalAlignment="Bottom" x:Name="BottomPart" Margin="1 0 1 1" Opacity="0"
                                DataContext="{TemplateBinding SelectedMediaItem}" Background="#01000000">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="29" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <Border Grid.ColumnSpan="3" Grid.Row="1"
                                    BorderThickness="{StaticResource MediaPlayerBottomPart_BorderThickness}"
                                    CornerRadius="{StaticResource MediaPlayerBottomPart_CornerRadius}"
                                    Background="{StaticResource MediaPlayerBottomPart_Background}"
                                    BorderBrush="{StaticResource MediaPlayerBottomPart_BorderBrush}" />

                            <telerik:RadToggleButton Margin="10 0 0 0" x:Name="PlayPauseToggle"
                                    Template="{StaticResource PlayPauseButtonTemplate}" HorizontalAlignment="Left"
                                    Grid.Row="1" Command="player:MediaCommands.TogglePlayPause"
                                    CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=IsChecked}" />

                            <Rectangle Height="8" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" RadiusX="1"
                                    RadiusY="1" Grid.Column="1" Grid.Row="1" Margin="10,0,0,-1"
                                    Stroke="{StaticResource MediaPlayer_Slider_OuterBorderBrushThump}"
                                    Fill="{StaticResource MediaPlayer_Slider_OuterBackgroundThump}" />

                            <Border x:Name="ChaptersPresenterBackground" Grid.ColumnSpan="3" Visibility="Collapsed"
                                    BorderThickness="{StaticResource MediaChapters_BorderThickness}"
                                    CornerRadius="{StaticResource MediaChapters_CornerRadius}"
                                    BorderBrush="{StaticResource MediaChapters_BorderBrush}"
                                    Background="{StaticResource MediaChapters_Background}" />

                            <Border x:Name="ChaptersPresenter" Grid.Column="1" Margin="10 0 0 0">
                                <ContentPresenter x:Name="ChaptersContentPresenter"
                                        Content="{Binding ChaptersPresenter}" />
                            </Border>

                            <telerik:RadProgressBar Style="{StaticResource DownloadProgressBar}"
                                    x:Name="DownloadProgress" Minimum="0" Maximum="1" Margin="10 0 0 0" Height="8"
                                    Grid.Column="1" Grid.Row="1" />

                            <telerik:RadSlider x:Name="TimeSlider" EnableSideTicks="False"
                                    Maximum="{TemplateBinding MediaElementTotalSeconds}"
                                    Value="{Binding Position, Mode=TwoWay}" TickPlacement="TopLeft"
                                    Style="{StaticResource TimeSliderStyle}" Margin="10 0 0 0"
                                    IsMoveToPointEnabled="True" TickFrequency="0" BorderThickness="0" Grid.Column="1"
                                    Grid.Row="1" IsEnabled="{TemplateBinding CanSeek}" />

                            <StackPanel Width="Auto" Orientation="Horizontal" HorizontalAlignment="Right"
                                    VerticalAlignment="Center" Grid.Column="2" Grid.Row="1">

                                <!-- separator -->
                                <Border Width="2" Margin="10 -2" BorderThickness="1 0 0 0"
                                        BorderBrush="{StaticResource MediaPayer_SeparatorBorder}"
                                        Background="{StaticResource MediaPayer_SeparatorBorder1}" />

                                <!-- Text="{TemplateBinding MediaElementCurrentTime}" -->
                                <TextBlock Text="{TemplateBinding MediaElementCurrentTime}"
                                        FontFamily="{TemplateBinding FontFamily}" TextWrapping="NoWrap"
                                        FontSize="{TemplateBinding FontSize}" Foreground="{TemplateBinding Foreground}"
                                        VerticalAlignment="Center" Padding="0,0,2,0">
                                </TextBlock>
                                <TextBlock Text="/" TextWrapping="NoWrap" FontFamily="{TemplateBinding FontFamily}"
                                        FontSize="{TemplateBinding FontSize}" Foreground="{TemplateBinding Foreground}"
                                        VerticalAlignment="Center" />
                                <!-- Text="{TemplateBinding MediaElementTotalTime}"  -->
                                <TextBlock Text="{TemplateBinding MediaElementTotalTime}" TextWrapping="NoWrap"
                                        FontFamily="{TemplateBinding FontFamily}" FontSize="{TemplateBinding FontSize}"
                                        Foreground="{TemplateBinding Foreground}" VerticalAlignment="Center"
                                        Margin="2,0,0,0" />

                                <!-- separator -->
                                <Border Width="2" Margin="10 -2" BorderThickness="1 0 0 0"
                                        BorderBrush="{StaticResource MediaPayer_SeparatorBorder}"
                                        Background="{StaticResource MediaPayer_SeparatorBorder1}" />

                                <!-- Mute -->
                                <telerik:RadToggleButton Command="player:MediaCommands.ToggleMuteOnOff"
                                        Template="{StaticResource MuteButtonTemplate}"
                                        CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=IsChecked}"
                                        Width="15.5" Height="12" HorizontalAlignment="Left" VerticalAlignment="Center"
                                        Margin="0 0 5 0" />

                                <telerik:RadSlider x:Name="VolumeSlider"
                                        Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Volume, Mode=TwoWay}"
                                        Minimum="0" Maximum="1" LargeChange="0.1" SmallChange="0.1" Width="53.132"
                                        Style="{StaticResource VolumeSliderStyle}" VerticalAlignment="Center"
                                        IsMoveToPointEnabled="True" EnableSideTicks="False" HorizontalAlignment="Left"
                                        Margin="0" Height="Auto" />

                                <!-- separator -->
                                <Border Width="2" Margin="10 -2 0 -2" BorderThickness="1 0 0 0"
                                        BorderBrush="{StaticResource MediaPayer_SeparatorBorder}"
                                        Background="{StaticResource MediaPayer_SeparatorBorder1}" />

                                <telerik:RadButton Padding="5" VerticalAlignment="Center" Width="42"
                                        Command="player:MediaCommands.ToggleFullScreen"
                                        Template="{StaticResource MediaPlayer_Button}">
                                    <telerik:RadButton.Content>
                                        <Grid Width="16" Height="12" VerticalAlignment="Center"
                                                HorizontalAlignment="Center">
                                            <Border HorizontalAlignment="Right" Margin="0,0,0,2" Width="14"
                                                    BorderThickness="1"
                                                    BorderBrush="{StaticResource MediaPlayer_ButtonBorderPath}"
                                                    CornerRadius="1" VerticalAlignment="Bottom" Height="10"
                                                    Background="{StaticResource MediaPlayer_ButtonBackgroundPath}" />
                                            <Border HorizontalAlignment="Right" Margin="0,0,8,0" Width="8"
                                                    BorderThickness="1"
                                                    BorderBrush="{StaticResource MediaPlayer_ButtonBorderPath}"
                                                    CornerRadius="1" VerticalAlignment="Bottom" Height="6"
                                                    Background="{StaticResource MediaPlayer_ButtonBackgroundPath}" />
                                        </Grid>
                                        <!--<Path Stretch="Fill" Width="14" Height="12"
												Fill="{StaticResource MediaPlayer_ButtonBackgroundPath}"
												Data="M7.04,0.99999965 L7.04,5.041 1,5.041 1,8.9639993 12.249,8.9639993 12.249,0.99999965 z M0.25000001,0 L13.25,0 13.25,0.99999965 13.249,0.99999965 13.249,8.9639993 13.25,8.9639993 13.25,9.9639993 13.249,9.9639993 12.249,9.9639993 1,9.9639993 0.25000001,9.9639993 0,9.9639993 0,0.040999879 0.04000004,0.040999879 0.25000001,0.040999879 z" />-->
                                    </telerik:RadButton.Content>
                                </telerik:RadButton>

                                <!-- separator -->
                                <Border Width="2" Margin="0 -2" BorderThickness="1 0 0 0"
                                        BorderBrush="{StaticResource MediaPayer_SeparatorBorder}"
                                        Background="{StaticResource MediaPayer_SeparatorBorder1}" />

                                <!-- Playlist -->
                                <telerik:RadButton Padding="5" VerticalAlignment="Center" Width="42"
                                        Command="player:MediaCommands.TogglePlaylist"
                                        Template="{StaticResource MediaPlayer_Button}">
                                    <telerik:RadButton.Content>
                                        <StackPanel>
                                            <Rectangle Stroke="{StaticResource MediaPlayer_ButtonBorderPath}"
                                                    StrokeThickness="1" Width="12" Height="3"
                                                    Fill="{StaticResource MediaPlayer_ButtonBackgroundPath}" />
                                            <Rectangle Stroke="{StaticResource MediaPlayer_ButtonBorderPath}"
                                                    StrokeThickness="1" Width="12" Height="3" Margin="0 1"
                                                    Fill="{StaticResource MediaPlayer_ButtonBackgroundPath}" />
                                            <Rectangle Stroke="{StaticResource MediaPlayer_ButtonBorderPath}"
                                                    StrokeThickness="1" Width="12" Height="3"
                                                    Fill="{StaticResource MediaPlayer_ButtonBackgroundPath}" />
                                        </StackPanel>
                                    </telerik:RadButton.Content>
                                </telerik:RadButton>
                            </StackPanel>
                        </Grid>
                        <Rectangle Fill="{StaticResource MediaPayer_DisabledBrush}" RadiusX="8" RadiusY="8"
                                x:Name="DisabledVisual" Visibility="Collapsed" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    

</ResourceDictionary>
