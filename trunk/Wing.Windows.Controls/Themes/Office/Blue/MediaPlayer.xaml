    <!--Thumb for VolumeSlider-->

    <!--TimeSlider Thumb Style-->
    <ControlTemplate x:Key="TimeSliderThumbTemplate" TargetType="Thumb">
        <Grid>
            <Border Background="{StaticResource MediaPalyer_SliderThumb_Bacrgound}"
                    BorderBrush="{StaticResource MediaPalyer_SliderThumb_BorderBrush}"
                    CornerRadius="{StaticResource MediaPalyer_SliderThumb_CornerRadius}"
